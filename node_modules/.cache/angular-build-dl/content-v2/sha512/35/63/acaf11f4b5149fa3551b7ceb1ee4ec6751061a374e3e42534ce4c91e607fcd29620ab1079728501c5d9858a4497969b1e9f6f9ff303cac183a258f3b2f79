     if (this.tabMap[this.tab] == 'people' || this.tabMap[this.tab] == 'Sex' || this.tabMap[this.tab] == 'country' || this.tabMap[this.tab] == 'Sexual_Orientation' || \n                                this.tabMap[this.tab] == 'interested') {\n                                    for (var i = 0; i < data['data'].length; i++) {\n                                        data['data'][i].user_type = 'false';\n                                        var loopdata = data['data'][i].followers;\n                                        for (var j = 0; j < loopdata.length; j++) {\n                                             if (loopdata[j].user_id == this.user_id) {\n                                                data['data'][i].user_type = 'true';\n                                            }\n                                        }\n                                    }\n                                    this.searched = data['data'];\n                                    setTimeout(() => {\n                                        this.profileLoading = false;\n                                    }, 3000);\n\n                                } else {\n                                    this.profileLoading = false;\n                                    this.searched = data['data'];\n                                    setTimeout(() => {\n                                        this.profileLoading = false;\n                                    }, 3000);\n                                }\n\n                            }\n                        });\n                } else {\n                    this.searched = [];\n                  \n                }\n             })\n         );\n    }\n\nemptySearch(){\n    if(this.inputValue == ''){\n        this.searched = [];\n    }\n}\n\n    GetMyProfile() {\n        this.profileService.GetProfileSub().subscribe((data) => {\n            if (data && data['success']) {\n                this.isblocked = data['blocked'];\n            }\n        });\n    }\n    GenderValue = [];\n\n    TabChange(e) {\n        this.searched = [];\n        this.tab = e;\n        if (this.tab == 4) {\n            this.GenderValue = Sex;\n\n        }\n        else if (this.tab == 6) {\n            this.GenderValue = Sexual;\n\n        }\n        else if (this.tab == 5) {\n            this.GenderValue = CountryDropdown;\n\n        }\n        else if(this.tab == 7){\n            this.GenderValue = this.categoryList;\n        }\n        this.Search(this.inputValue);\n    }\n\n    Search(val) {\n        if (val == '') {\n            this.profileLoading = false;\n        }\n        else {\n            this.profileLoading = true;\n        }\n        this.searchTextChanged.next(val);\n    }\n\n    GetList(){\n        this.searchService.RecentAll().subscribe((res)=>{\n                if (res && res['success']) {\n                    this.recentUserSearch = res['data'].search;\n                    this.recentUserSearch = this.recentUserSearch.reverse();\n                }\n        })\n    }\n\n    ClearAll(){\n        this.searchService.ClearAll().subscribe((res)=>{\n            \n        })\n    }\n\n    CategoryList(){\n        this.profileService.CategoryList(localStorage.getItem('locale')).subscribe((res)=>{\n            if(res['success']){\n                this.categoryList = res['data'];\n            }\n        })\n    }\n}\n","<div class=\"main-layout\">\n    <div *ngIf=\"isblocked\">\n        <div class=\"layout-inner-body d-flex justify-content-center\">\n            <nz-result nzStatus=\"info\" nzTitle=\"User blocked\">\n                <div nz-result-extra>\n                    {{\"Blocked\" | translate}}\n                </div>\n            </nz-result>\n        </div>\n    </div>\n    <div class=\"layout-body\" *ngIf=\"!isblocked\">\n        <div class=\"layout-inner-body\">\n            <div class=\"layout-feed\">\n                <div class=\"search-content\">\n                    <div class=\"search-header\">\n     