\n\n    DeleteSchedule(id) {\n        this.socketService\n            .DeleteCall({ schedule_id: id })\n            .subscribe((res) => {\n                if (res['success']) {\n                    this.message.success(res['message']);\n                    this.GetCallSchedules();\n                } else {\n                    this.message.error(res['message']);\n                }\n            });\n    }\n\n    cancelDeleteSchedule(id){\n        this.socketService\n            .userDeleteCall({ schedule_id: id })\n            .subscribe((res) => {\n                if (res['success']) {\n                    this.message.success(res['message']);\n                    this.GetCallSchedules();\n                } else {\n                    this.message.error(res['message']);\n                }\n            });\n    }\n\n    availableList() {\n        if (this.available == true) {\n            this.scheduleList = [];\n            this.scheduleList = this.availablescheduleList;\n            this.available = false;\n        } else {\n            this.GetCallSchedules();\n        }\n    }\n\n    BlockedCall(){\n      this.message.error('User blocked by admin')\n    }\n   \n}\n","<div class=\"list-wrapper\">\n    <div class=\"list-overflow scroll-bar-none\">\n        <div class=\"list-body\">\n            <div class=\"notificationallpage\">\n                <div class=\"notificationcallcontent\">\n                    <div class=\"callvideo\">\n                        <div class=\"rightnotification p-15 border-bottom\" (click)=\"availableList()\">\n                            <div class=\"d-flex align-items-center\">\n                                <div class=\"videocall\" *ngIf=\"available == true\">\n                                    {{availablescheduleList.length}}\n                                </div>\n                                <div class=\"videocontent\">\n                                    <h6 class=\"mb-0\" *ngIf=\"available == true\">{{\"AvailableCall\" | t