nguage: any;\n    catename: any;\n    GetMyProfile() {\n        this.profileService.GetProfile().subscribe((data) => {\n            if (data && data['success']) {\n                this.myProfie = data['data'];\n                this.display_name = this.myProfie.display_name;\n                this.isblocked = this.myProfie.blocked;\n                this.mybalance = this.myProfie.balance;\n                this.language = this.myProfie.language;\n                this.catename = this.myProfie.categoryname;\n            }\n        });\n    }\n\n    bookmarkthispost(post_id, i) {\n        if (this.listFeedPost[i].isMarked == true) {\n            this.listFeedPost[i].isMarked = false;\n            this.homeservice.RemoveBookMarkPost({ post_id: post_id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.listFeedPost[i].bookmarks = data['data'].bookmarks;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        } else {\n            this.listFeedPost[i].isMarked = true;\n            this.homeservice.BookMarkPost({ post_id: post_id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.listFeedPost[i].bookmarks = data['data'].bookmarks;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        }\n    }\n\n    AddTips(id, amount, i) {\n        this.isVisibleTips = false;\n        this.addtipsTemp.post_id = id;\n        this.addtipsTemp.amount = amount;\n        this.addtipsTemp.i = i;\n        this.isVisibleTipsConfirmation = true;\n        this.tipsAmount = 0;\n    }\n\n    handleCancelTipsConfirm() {\n        this.isVisibleTipsConfirmation = false;\n        this.tipsAmount = 0;\n    }\n\n    ConfirmTipsPay() {\n        thi