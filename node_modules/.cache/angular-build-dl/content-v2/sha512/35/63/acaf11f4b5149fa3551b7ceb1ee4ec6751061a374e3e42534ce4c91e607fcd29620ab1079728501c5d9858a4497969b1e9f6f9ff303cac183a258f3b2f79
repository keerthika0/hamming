on = true;\n        this.imageChangedEvent = event;\n        this.pic_type = type;\n\n    }\n\n    imageCropped(event: ImageCroppedEvent) {\n        // this.croppedImage = event.base64;\n        if (this.pic_type == \"profile_image\") {\n            this.preview['profile_image'] = event.base64;\n            this.profile_pic = event.base64;\n        }\n\n        if (this.pic_type == \"cover_image\") {\n            this.preview['cover_image'] = event.base64;\n            this.cover_pic = event.base64;\n        }\n    }\n    imageLoaded() {\n        // show cropper\n    }\n    cropperReady() {\n        // cropper ready\n    }\n    loadImageFailed() {\n        // show message\n    }\n    save_profile_img() {\n        this.savebutton = false;\n\n        var format;\n        if (this.pic_type == \"profile_image\") {\n            format = {\n                profile_pic: this.profile_pic\n            }\n        }\n        if (this.pic_type == \"cover_image\") {\n            format = {\n                cover_pic: this.cover_pic\n            }\n        }\n\n        this.profileService\n            .UpdateProfilePic(format)\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(\n                            data['message']\n                        );\n                        this.profileService.changedPic(data['data']);\n\n                    } else {\n                        this.notify.error(\n                            data['message']\n                        );\n                    }\n                },\n                (err) => {\n                    this.notify.error(\n                        err.error['message']\n                    );\n                }\n            );\n\n\n    }\n\n    copiednotify() {\n        this.copy = 'true'\n    }\n    categoryList() {\n        this.profileService.CategoryList(this.lang).subscribe((res) => {\n            this.category = res['data']\n        })\n    }\n\n    subcat(ind) {\n         this.sub_cat = null;\n        for (let i = 0; i < this.category.length; i++) {\n            if (this.category[i]['_id'] == ind) {\n                this.Cat_id = this.category[i]['id'];\n                // this.isShow = this.category[i]['adult'];\n                // this.subcategory = this.category[i]['sub_category'];\n                // this.category_name = this.category[i]['categoryname'];\n                this.subcatlist();\n            }\n        }\n\n    }\n    \n    getSubid(ind) {\n        this.Cat_id = this.categoryName\n        this.categoryList();\n        for (let i = 0; i < this.category.length; i++) {\n            if (this.category[i]['_id'] == ind) {\n                this.Cat_id = this.category[i]['id'];\n                this.subcatlist();\n            }\n        }\n        setTimeout(() => {\n            for (let i = 0; i < this.subcategory.length; i++) {\n                if (this.subcategory[i]['_id'] == ind) {\n                    this.Sub_id = this.subcategory[i]['id'];\n                }\n            }\n        }, 3000);\n\n    }\n\n    subcatlist() {\n        this.profileService.SubCatList(this.Cat_id, this.lang).subscribe((res) => {\n            this.subcategory = res['data'];\n         })\n    }\n\n    subcatvalue(ind) {\n         for (let i = 0; i < this.category.length; i++) {\n            if (this.category[i]['_id'] == ind) {\n                this.Cat_id = this.category[i]['id'];\n            this.subcatlist();\n            }\n        }\n        this.sub_cat = this.SubcategoryName;\n\n    }\n\n    GetProfile() {\n        this.subscriptions.push(\n            this.profileService.GetProfileSub().subscribe((data) => {\n                if (data && data['success']) {\n                    if (this.packages.length == 0) this.GetPremiumPackages();\n                    this.SetSubscriptionForm(data);\n                    this.Country();\n                    this.profile = data;\n                    this.isblocked = this.profile.blocked;\n                    this.ispremium = data['ispremiumuser'];\n                    this.user_name = data['user_name']\n                    this.lang = data['language'];\n                     this.Sex = data['sex'];\n                    this.Sexual_Orientation = data['sexual_orientation'];\n                    this.country = data['country'];\n                    this.mobile = data['mobile'];\n                    this.country_name = data['country_name'];\n                    this.categoryName = data['category_id']['_id'];\n                    this.sub_cat = data['subcategory_id']['_id'];\n                    this.SubcategoryName = data['subcategory_id']['_id'];\n                    \n                }\n            })\n        );\n    }\n\n    GetPremiumPackages() {\n        this.subscriptionService.GetPremiumPackages().subscribe((data) => {\n            if (data && data['success']) {\n                // if (this.profile.trial_status == 1)\n                //     this.packages = data['data'].filter(\n                //         (data) => data.price != 0\n                //     );\n                // else \n                this.packages = data['data'];\n                this.SelectPackage(0);\n            }\n        });\n    }\n\n    checkDuplicateData(value, type) {\n        this.profileService\n            .AuthDuplicate({ value: value, type: type })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        if (data['found']) {\n                            this.user_name_error = true;\n                        } else {\n                            this.user_name_error = false;\n                        }\n                    }\n                },\n                (err) => { }\n            );\n    }\n    callForm: FormGroup;\n    ngOnInit(): void {\n        this.callForm = this.fb.group({\n            duration: [null, [Validators.required, Validators.pattern('^(0|\\-?[1-9][0-9]*)$'),