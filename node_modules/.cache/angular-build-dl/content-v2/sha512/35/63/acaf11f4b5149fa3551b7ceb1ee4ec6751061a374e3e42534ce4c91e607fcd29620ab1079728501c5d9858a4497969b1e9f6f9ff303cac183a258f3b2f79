\n        this.depositamount = this.addAmount;\n        this.addAmount = 0;\n    }\n\n\n    GetCardList() {\n        this.walletservice.GetCardList().subscribe((data) => {\n            if (data['success']) {\n                this.listOfCard = data['data'];\n                if (data['data'].length == 0) {\n                    this.cardnotfound = true;\n                }\n            }\n        },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    GetPaymentDetails(amount, coin) {\n        this.selectCoin = coin;\n\n        this.walletservice.GetPaymentDetails({\n            amount: amount,\n            payment_type: coin.toUpperCase(),\n        }).subscribe((data) => {\n            if (data['success']) {\n                this.deposit = 'cryptoPayment';\n                this.getPaymentDetails = data['data'];\n            }\n        },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    // Modal -- Tips\n    isVisibleTips = false;\n    showTips(): void {\n        this.isVisibleTips = true;\n    }\n    handleOkTips(): void {\n        this.isVisibleTips = false;\n    }\n\n    handleCancelTips(): void {\n        this.isVisibleTips = false;\n    }\n\n    getProfile() {\n        this.walletservice.GetProfile().subscribe((data) => {\n            if (data['success']) {\n                // console.log(data['data'].balance);\n                this.walletBalance = data['data'].balance;\n                    \n            }\n        },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    addMoney(amount, id) {\n       if (id) {\n            this.walletservice\n                .AddMoney({\n                    amount: amount,\n                    id: id,\n                })\n                .subscribe(\n                    (data) => {\n                        if (data['success']) {\n                            this.notify.success(data['message']);\n                            this.getProfile();\n                            this.handleCancelDeposit();\n                            this.amount = 0;\n                            location.reload();\n                        } else {\n                            this.notify.error(data['message']);\n                        }\n                    },\n                    (err) => {\n                        this.notify.error(err.error['message']);\n                    }\n                );\n        } else {\n            this.notify.error(\" Select Card \");\n        }\n    }\n\n    Notify() {\n        this.notify.success(\"Copied\", \"\");\n    }\n}\n","<div class=\"page-header justify-content-between\">\n    <div class=\"page-title\">{{\"WalletCredits\" | translate}}</div>\n</div>\n<div class=\"edit-form\">\n    <div class=\"layout-inner-body walletcreditspage\">\n        <div class=\"walletcreditscontent\">\n            <div class=\"walletcredits-card mb-4\">\n                <div class=\"walletcredits-amounts mb-4\">\n                    <div class=\"walletcredits-text\">\n                        <h5 class=\"mb-0\">{{\"Balance\" |translate}}</h5>\n                    </div>\n                    <div class=\"walletcredits-balance\">\n                        <h4 class=\"mb-0 text-primary\">€{{walletBalance | number: \"1.2-2\"}}</h4>\n                    </div>\n                </div>\n                <div class=\"walletcredits-addfund pb-4\">\n                    <nz-form-label>{{\"AddFund\" | translate}}</nz-form-label>\n                    <input nz-input placeholder=\"€\" type=\"number\" value=\"{{amount}}\" name=\"amount\" id=\"amount\"\n                        #depamount [formControl]=\"deposit_amount\" />\n                    <div *ngIf=\"deposit_amount.hasError('pattern')\" style=\"color:red\">\n                        {{\"InvalidAmt\" | translate}}.\n                    </div>\n\n                 