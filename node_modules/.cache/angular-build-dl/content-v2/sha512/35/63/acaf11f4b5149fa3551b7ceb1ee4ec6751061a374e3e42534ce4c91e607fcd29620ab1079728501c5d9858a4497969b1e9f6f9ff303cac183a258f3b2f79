ice = this.profile.calltimings;\n            if (callprice.length == 0) {\n                this.profileService\n                    .AddCallPricing({ duration: duration, price: price })\n                    .subscribe(\n                        (data) => {\n                            if (data['success']) {\n                                this.notify.success(data['message']);\n                                this.profile.calltimings = data['data'].calltimings;\n                                this.cancelPricing();\n                            }\n                        },\n                        (err) => {\n                            this.notify.error(err.error.message);\n                            this.cancelPricing();\n                        }\n                    );\n            }\n\n            for (let i = 0; i < callprice.length; i++) {\n                if (callprice[i].duration == duration || callprice[i].price == price) {\n                    this.notify.error('Entered value is already present');\n                    return true;\n                }\n                else {\n                    this.profileService\n                        .AddCallPricing({ duration: duration, price: price })\n                        .subscribe(\n                            (data) => {\n                                if (data['success']) {\n                                    this.notify.success(data['message']);\n                                    this.profile.calltimings = data['data'].calltimings;\n                                    this.cancelPricing();\n                                }\n                            },\n                            (err) => {\n                                this.notify.error(err.error.message);\n                                this.cancelPricing();\n                            }\n                        );\n                    return true;\n                }\n            }\n\n\n        }\n    }\n\n    DeletePricing(id, i) {\n        