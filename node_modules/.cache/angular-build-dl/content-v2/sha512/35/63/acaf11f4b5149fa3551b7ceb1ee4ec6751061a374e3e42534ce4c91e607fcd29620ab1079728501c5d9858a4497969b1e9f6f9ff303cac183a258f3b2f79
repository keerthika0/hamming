                      this.posts.push(...data['data']);\n                            this.totalPosts = data['count'];\n                        }\n                    }\n                });\n        }\n    }\n\n    SetSubscriptionForm(data: IProfile) {\n        this.subscriptionForm = this.fb.group({\n            subscription_price: [\n                data.subscription_price,\n                [Validators.required, Validators.min(0)],\n            ],\n            subscription_price_6m: [\n                data.subscription_price_6m,\n                [Validators.required, Validators.min(0)],\n            ],\n            subscription_price_3m: [\n                data.subscription_price_3m,\n                [Validators.required, Validators.min(0)],\n            ],\n            subscription_price_1yr: [\n                data.subscription_price_1yr,\n                [Validators.required, Validators.min(0)],\n            ],\n        });\n        this.oneMonth = data.subscription_price_status;\n        this.threeMonth = data.subscription_price_3m_status;\n        this.sixMonth = data.subscription_price_6m_status;\n        this.oneYear = data.subscription_price_1yr_status;\n    }\n\n\n    Updateprofile() {\n        for (const i in this.profileForm.controls) {\n            this.profileForm.controls[i].markAsDirty();\n            this.profileForm.controls[i].updateValueAndValidity();\n        }\n\n        this.profileService\n            .AuthDuplicate({\n                value: this.profileForm.value['user_name'],\n                type: 'user_name',\n            })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        if (data['found']) {\n                            this.user_name_error = true;\n                        } else {\n                            this.user_name_error = false;\n                            if (this.profileForm.valid) {\n                                const formData = new FormData();\n