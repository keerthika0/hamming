\">\n                        <div class=\"withdraw-time\">\n                            {{withdraw.createdAt | dateAgo}}\n                        </div>\n                        <div class=\"withdraw-status\" class=\"pending\" *ngIf=\"withdraw.status == 2\">\n                            {{\"Pending\" | translate}}</div>\n                        <div class=\"withdraw-status\" class=\"pending\" *ngIf=\"withdraw.status == 0 && withdraw.notes\">\n                            {{\"Rejected\" | translate}}\n                        </div>\n                        <div class=\"withdraw-status\" class=\"pending\" *ngIf=\"withdraw.status == 0 && !withdraw.notes\">\n                            {{\"Cancelled\" | translate}}\n                        </div>\n                        <div class=\"withdraw-status\" class=\"success\" *ngIf=\"withdraw.status == 1\">\n                            {{\"success\" | translate}}</div>\n                    </div>\n                </div>\n            </div>\n            <div *ngIf=\"withdrawhistory?.length == 0\" class=\"text-center pt-5\">\n               {{\"NoItem\" | translate}}\n            </div>\n        </div>\n    </div>\n</div>\n<!-- Modal -- Withdraw Cancel -- Start -->\n<nz-modal nzWrapClassName=\"vertical-center-modal custom-modal\" [(nzVisible)]=\"isVisibleWithdrawCancel\"\n    (nzOnCancel)=\"handleCancelWithdrawCancel()\" (nzOnOk)=\"handleOkWithdrawCancel()\" [nzFooter]=\"null\"\n    nzClosable=\"false\">\n    <div class=\"custom-modal-header\">\n        <div class=\"custom-modal-title\">\n            {{\"WithdrawCancel\" | translate}}\n        </div>\n        <button nz-button class=\"modal-close-btn\" (click)=\"handleCancelWithdrawCancel()\">\n            <i-feather name=\"x\"></i-feather>\n        </button>\n    </div>\n    <div class=\"custom-modal-body\">\n        <div class=\"text-left\">\n            <p class=\"my-3\">{{\"ConfirmWithdrawCancel\" | translate}}</p>\n        </div>\n    </div>\n    <div class=\"custom-modal-fo