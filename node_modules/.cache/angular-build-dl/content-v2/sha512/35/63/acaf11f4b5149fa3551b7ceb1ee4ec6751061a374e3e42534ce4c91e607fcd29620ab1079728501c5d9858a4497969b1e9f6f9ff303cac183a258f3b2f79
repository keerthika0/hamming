               this.loading = true;\n                if(this.datahited == false){\n                    this.datahited = true;\n                    this.HistoryonScroll();\n                }\n            }\n        }\n    }\n\n\n    HistoryonScroll() {\n        if (this.count > 0) {\n            if (this.count >= this.limit) {\n                this.count = this.count - this.limit;\n            }\n            if (this.count < this.limit && this.count != 0) {\n                this.limit = this.count;\n                this.count = this.count - this.limit;\n            }\n            if (this.limit != 0) {\n                this.walletservice\n                .GetPaymentHistoryListById(this.id, this.limit)\n                .subscribe(\n                    async (data) => {\n                        if (data['success']) {\n\n                            this.loading = false;\n\n                            for (var i = 0; i < data['data'].length; i++) {\n                                this.paymentHistory.push(data['data'][i]);\n                            }\n                            this.id = data['data'].length - 1;\n                            this.id = data['data'][this.id]._id;\n                            this.datahited = false;\n                        }\n                    },\n                    (err) => {\n                        this.notify.error(err.error.message);\n                    }\n                    );\n            }else{\n                this.loading = false;\n            }\n        }else{\n           this.loading = false; \n        }\n    }\n\n}\n","import { Component, OnInit } from '@angular/core';\nimport { ProfileService } from 'src/app/services/profile.service';\n\n@Component({\n    selector: 'app-wallet',\n    templateUrl: './wallet.component.html',\n    styleUrls: ['./wallet.component.less'],\n})\nexport class WalletComponent implements OnInit {\n    // Tab\n    tab = 0;\n    isblocked: any\n    constructor(private profileService: ProfileServi