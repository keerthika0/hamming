    fetching = false;\n    limit = 5;\n    premium: any;\n    editLoading = false;\n\n    appUrl = Constants.baseUrl2;\n\n    isVisibleDeletePost = false;\n    isVisibleEditPost = false;\n\n    tempPost: IPost;\n\n    editFiles = [];\n    text = '';\n    hide = false;\n    Temphide = '';\n\n    followers = [];\n    following = [];\n\n    suggestionList: Array<Object>;\n    completeList: Array<Object>;\n    selcteduser: any = {};\n    selectedUserOnTimeCoupon: any = {};\n    isVisibleSubscribe = false;\n    showMore = true;\n    myProfie: IProfile;\n    subscribeProfile: IProfile;\n    isVisiblePremiumSub = false;\n    listOfData = [];\n    fcount: any;\n    ccode: any;\n    coupon_per: any = 0;\n    coupon_id: any;\n    payPrice: any = 0;\n    errmessage: any = ' ';\n    subscription_price = 0;\n    dis_amount: any = 0;\n    selectedIndex: any;\n    selectStatus = 'active';\n    renew_id: any;\n    followerCount = 0;\n    showModalSubscribe(usr, i): void {\n        this.subscribe = 'subscribeSelect';\n        this.renew_id = i;\n        this.selcteduser = usr;\n        this.selectedUserOnTimeCoupon = usr;\n        this.isVisibleSubscribe = true;\n    }\n\n    handleOkSubscribe(): void {\n        this.isVisibleSubscribe = false;\n    }\n\n    handleCancelSubscribe(): void {\n        this.selectedIndex = null;\n        this.isVisibleSubscribe = false;\n    }\n\n    // Radio -- Subscribe Pack\n    subscribeValue = 'A';\n    subscribeSelectPayment = 'wallet';\n    isblocked: any;\n    // Modal -- Subscribe -- selected view\n    subscribe = 'subscribeSelect';\n\n    // Modal -- Unsubscribe\n    isVisibleUnsubscribe = false;\n    showModalUnsubscribe(): void {\n        this.isVisibleUnsubscribe = true;\n    }\n\n    handleOkUnsubscribe(): void {\n        this.isVisibleUnsubscribe = false;\n    }\n\n    handleCancelUnsubscribe(): void {\n        this.isVisibleUnsubscribe = false;\n    }\n\n    constructor(\n        private profileService: ProfileService,\n        private postService: PostService,\n        private notify: NotifyService,\n        private message: NzMessageService,\n        private sanitizer: DomSanitizer,\n        private offersservice: OffersService,\n        private listService: ListService\n    ) { }\n\n    ngOnInit() {\n        let printSrc = new PrintScreens();\n        printSrc.PrintScr();\n        this.GetProfile();\n        this.GetMyPosts();\n        this.GetFollowersAndFollowing();\n        this.SelectMenu();\n        this.followerselect();\n\n        this.listService.GetFollowCount().subscribe((res)=>{\n            if(res['success']){\n                this.followerCount = res['followingscount'];\n                this.fcount = res['follow'];\n            }\n        })\n\n    }\n\n    imageChangedEvent: any = '';\n    croppedImage: any = '';\n\n    // fileChangeEvent(event: any): void {\n    //     console.log(event);\n    //     this.imageChangedEvent = event;\n        \n\n    // }\n\n    fileChangeEvent(e: any): void {\n        if(e.target.files[0].type.split('/')[0] == \"image\"){\n            this.imageChangedEvent = e;\n        }\n        if(e.target.files[0].type.split('/')[0] == \"video\"){\n            if (\n            this.editFiles.length == 10 ||\n            this.editFiles.length + e.target.files.length > 10\n        ) {\n            return;\n        }\n        if (e.target.files && e.target.files[0]) {\n            for (let i = 0; i < e.target.files.length; i++) {\n                var reader = new FileReader();\n\n                reader.onload = (data: any) => {\n                    this.editFiles.push({\n                        url: this.sanitizer.sanitize(\n                            SecurityContext.URL,\n                            this.sanitizer.bypassSecurityTrustUrl(\n                                data.target.result\n                            )\n                        ),\n                        ptype: e.target.files[i].type.split('/')[0],\n                        u: true,\n                    });\n                };\n\n                reader.readAsDataURL(e.target.files[i]);\n            }\n        }\n        }\n    }\n\n\n    imageCropped(event: ImageCroppedEvent) {\n        this.croppedImage = event.base64;\n\n    }\n    imageLoaded() {\n        // show cropper\n    }\n    cropperReady() {\n        // cropper ready\n    }\n    loadImageFailed() {\n        // show message\n    }\n\n    cropDone(){\n        var reader = new FileReader();\n        this.editFiles.push({\n             url: this.sanitizer.sanitize(\n                            SecurityContext.URL,\n                            this.sanitizer.bypassSecurityTrustUrl(\n                                this.croppedImage\n                            )\n                        ),\n                        ptype: \"image\",\n            u: true,\n        });\n\n       \n                        \n        var myBlob = new Blob([this.croppedImage], {type : \"image/png\"});\n\n        \n      \n        this.croppedImage='';\n        this.imageChangedEvent='';\n    }\n    cropCancel(){\n      \n        this.croppedImage='';\n        this.imageChangedEvent='';\n    }\n\n    onFileChange(e) {\n        // console.log(e);\n\n        if (\n            this.editFiles.length == 10 ||\n            this.editFiles.length + e.target.files.length > 10\n        ) {\n            return;\n        }\n        if (e.target.files && e.target.files[0]) {\n            for (let i = 0; i < e.target.files.length; i++) {\n                var reader = new FileReader();\n\n                reader.onload = (data: any) => {\n                    this.editFiles.push({\n                        url: this.sanitizer.sanitize(\n                            SecurityContext.URL,\n                            this.sanitizer.bypassSecurityTrustUrl(\n                                data.target.result\n                            )\n                        ),\n                        ptype: e.target.files[i].type.split('/')[0],\n                        u: true,\n                    });\n                };\n\n                reader.readAsDataURL(e.target.files[i]);\n            }\n        }\n    }\n\n    ngOnDestroy() {\n        this.subscriptions.forEach((item) => item.unsubscribe());\n    }\n\n    GetFollowersAndFollowing() {\n        this.profileService.GetFollowers().subscribe((data) => {\n            if (data['success']) {\n                this.followers = data['data'];\n            }\n        });\n    }\n\n    SelectMenu() {\n         if (this.selectStatus == 'active') {\n            this.listService.GetAllFollowing().subscribe((data) => {\n                if (data['success']) {\n                    this.following = data['result'];\n                    this.following = this.following.filter(\n                        (item) => item.plan_status === 1\n                    );\n                    // this.fcount = this.following.length;\n                }\n            });\n        } else if (this.selectStatus == 'expire') {\n            this.following = [];\n            this.listService.GetAllFollowing().subscribe((data) => {\n                if (data['success']) {\n                    this.following = data['result'];\n                    this.following = this.following.filter(\n                        (item) => item.plan_status === 0\n                    );\n                }\n            });\n        }\n        else if(this.selectStatus == 'follow'){\n            this.following = [];\n            this.listService.GetFollowList().subscribe(result => {\n                let val = result['data'];\n                 val.forEach(element => {\n                    this.following.push(element);\n                });\n\n            });\n        } else {\n            this.following = [];\n            this.listService.GetAllFollowing().subscribe((data) => {\n                if (data['success']) {\n                    this.following = data['result'];\n                }\n            });\n        }\n    }\n\n    GetProfile() {\n        this.subscriptions.push(\n            this.profileService.GetProfileSub().subscribe((data) => {\n                if (data && data['success']) {\n                    this.profile = data;\n                    this.isblocked = this.profile.blocked;\n                    this.premium = data['ispremiumuser'];\n                }\n            })\n        );\n    }\n\n    \n\n    EditSet(event) {\n        this.tempPost = { ...event };\n        this.editFiles = [...event.file];\n        this.text = event.oricontent;\n        this.hide = event.hide;\n        this.isVisibleEditPost = true;\n    }\n\n    dataURLtoFile(dataurl, filename) {\n        var arr = dataurl.split(','),\n            mime = arr[0].match(/:(.*?);/)[1],\n            bstr = atob(arr[1]),\n            n = bstr.length,\n            u8arr = new Uint8Array(n);\n\n        while (n--) {\n            u8arr[n] = bstr.charCodeAt(n);\n        }\n\n        return new File([u8arr], filename, { type: mime });\n    }\n\n    Delete() {\n        this.postService.Delete({ post_id: this.tempPost._id }).subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.notify.toast(data['message'], 'success');\n                    this.posts = this.posts.filter(\n                        (item) => item._id != this.tempPost._id\n                    );\n                    this.isVisibleDeletePost = false;\n                } else {\n                    this.notify.toast(data['message'], 'error');\n                }\n            },\n            (err) => {\n                this.notify.toast(err.error['message'], 'error');\n            }\n        );\n    }\n\n    GetMyPosts() {\n        if (\n            !this.fetching &&\n            (this.totalPosts == 0 || this.totalPosts > this.posts.length)\n        ) {\n            this.fetching = true;\n            this.postService\n                .GetMyPosts(\n                    this.posts.length > 0\n                        ? {\n                            limit: this.limit,\n                            id: this.posts.slice(-1)[0]._id || null,\n                        }\n                        : { limit: this.limit }\n                )\n                .pipe(finalize(() => (this.fetching = false)))\n                .subscribe((data) => {\n                    if (data['success']) {\n                        if (data['data'].length > 0) {\n                            this.posts.push(...data['data']);\n                            this.totalPosts = data['count'];\n                        }\n                    }\n                });\n\n            setTimeout(() => {\n                this.profileLoading = false;\n            }, 1200);\n        }\n    }\n\n    RemoveFile(indx) {\n        this.editFiles.splice(indx, 1);\n    }\n\n    addEmoji(e) {\n        this.text += e.emoji.native;\n        // this.ontextchange(this.text);\n    }\n\n    async EditPost() {\n        if (!this.text && !this.editFiles.length) return;\n        this.editLoading = true;\n        const formData = new FormData();\n\n        formData.append('post_id', this.tempPost._id);\n        formData.append('content', this.text);\n\n        formData.append('hide', this.hide.toString());\n        let fileChange = false;\n\n        if (\n            this.editFiles.length != this.tempPost.file.length ||\n            this.editFiles.some((item) => item.u)\n        ) {\n            fileChange = true;\n            for (let file of this.editFiles) {\n                if (!file.u) {\n                    const blob = await fetch(file.url).then((r) => r.blob());\n                    formData.append('file', blob);\n                } else {\n                    formData.append(\n                        'file',\n                        this.dataURLtoFile(\n                            file.url,\n                            Date.now() + this.tempPost._id\n                        )\n                    );\n                }\n            }\n        }\n\n        this.postService\n            .Edit(formData)\n            .pipe(finalize(() => (this.editLoading = false)))\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        // this.hide = 'false';\n\n                        this.posts = this.posts.map((item) => {\n                            if (item._id == this.tempPost._id) {\n                                item.content = this.text.replace(/#(\\w+)/g, '<a  href=\"/hashtag/$1\">#$1</a>');\n\n                                if (this.hide.toString() == 'false') {\n                                    item.hide = false;\n                                } else {\n                                    item.hide = true;\n                                }\n\n                                if (fileChange) {\n                                    const files = data['data'].file.map(\n                                        (element) => {\n                                            if (element.type) {\n                                                element.ptype = element.type.split(\n                                                    '/'\n                                                )[0];\n                                            }\n\n                                            return element;\n                                        }\n                                    );\n                                    item.file = files;\n                                }\n                            }\n                            return item;\n                        });\n\n                        this.notify.toast(data['message'], 'success');\n                        this.isVisibleEditPost = false;\n                    } else {\n                        this.notify.toast(data['message'], 'error');\n                    }\n                },\n                (err) => {\n                    this.notify.toast(err.error['message'], 'error');\n                }\n            );\n    }\n\n    getCouponList() {\n        this.offersservice.GetAvaiCoupons().subscribe((data) => {\n            if (data['success']) {\n                this.listOfData = data['data'];\n                if (this.subscribeValue == 'A') {\n                    this.payPrice = this.selcteduser.subscription_price;\n                } else if (this.subscribeValue == 'B') {\n                    this.payPrice = this.selcteduser.subscription_price_6m;\n                } else if (this.subscribeValue == 'D') {\n                    this.payPrice = this.selcteduser.subscription_price_3m;\n                } else if (this.subscribeValue == 'C') {\n                    this.payPrice = this.selcteduser.subscription_price_1yr;\n                }\n                let x = this.listOfData.find(\n                    (ob) => ob.couponcode == this.ccode\n                );\n                if (x) {\n                    this.coupon_id = x._id;\n                    this.coupon_per = x.percentage;\n                    this.errmessage = '';\n                    this.dis_amount = (this.payPrice * x.percentage) / 100;\n                } else {\n                    this.dis_amount = 0;\n                    this.errmessage = 'NO such Coupon Found';\n                }\n            }\n        });\n    }\n    //     rowno = 0;\n    //     addrow() {\n    //         this.rowno = this.rowno + 1;\n    //   }\n    getsubscribeValue() {\n        if (this.subscribeValue == 'A') {\n            this.subscription_price = this.selcteduser.subscription_price;\n        } else if (this.subscribeValue == 'B') {\n            this.subscription_price = this.selcteduser.subscription_price_6m;\n        } else if (this.subscribeValue == 'D') {\n            this.subscription_price = this.selcteduser.subscription_price_3m;\n        } else if (this.subscribeValue == 'C') {\n            this.subscription_price = this.selcteduser.subscription_price_1yr;\n        } else {\n            this.subscription_price = this.selcteduser.subscription_price;\n        }\n    }\n\n    ExtendUSer(i) {\n        // console.log(this.subscription_price);\n        let paymentprice = 0;\n        let paymentmonth = 0;\n        this.payPrice = this.subscription_price;\n        if (this.subscribeValue == 'A') {\n            paymentmonth = 1;\n        } else if (this.subscribeValue == 'B') {\n            paymentmonth = 6;\n        }\n        else if (this.subscribeValue == 'D') {\n            paymentmonth = 3;\n        } else if (this.subscribeValue == 'C') {\n            paymentmonth = 12;\n        }\n        let val = {\n            follower_id: this.selcteduser._id,\n            months: paymentmonth,\n            amount: this.payPrice,\n            coupon_code: this.ccode,\n            coupon_id: this.coupon_id,\n        };\n        if (this.profile.balance < this.payPrice) {\n            this.notify.error('Insufficient Fund')\n        }\n        else {\n            this.profileService.RenewSubscribeUser(val).subscribe((res) => {\n                if (res['success']) {\n                    this.message.success(res['message']);\n                    this.handleCancelSubscribe();\n                    this.SelectMenu();\n                    this.isVisibleFollowing = false;\n                } else {\n                    this.subscription_price = 0;\n                    this.message.error(res['message']);\n                }\n            });\n        }\n    }\n\n\n    getfollower = [];\n    selectfollowMenu = 'active'\n    followerselect() {\n        if (this.selectfollowMenu == 'expire') {\n            this.getfollower = [];\n            this.listService.GetexpiryFollower().subscribe(result => {\n                let val = result['data'];\n                // this.loading = false;\n                val.forEach(element => {\n                    this.getfollower.push(element);\n                });\n            });\n        }\n        else if (this.selectfollowMenu == 'active') {\n            this.getfollower = [];\n            this.listService.GetActiveFollower().subscribe(result => {\n                let val = result['data'];\n                // this.loading = false;\n                val.forEach(element => {\n                    this.getfollower.push(element);\n                });\n                // this.followerCount = this.getfollower.length;\n\n            });\n        }\n        else if (this.selectfollowMenu == 'follow') {\n            this.getfollower = [];\n            this.listService.GetFollowerList().subscribe(result => {\n                let val = result['data'];\n                 val.forEach(element => {\n                    this.getfollower.push(element);\n                });\n             });\n        }\n        else {\n            this.getfollower = [];\n            this.listService.GetFollower().subscribe(result => {\n                let val = result['data'];\n                // this.loading = false;\n                val.forEach(element => {\n                    this.getfollower.push(element);\n                });\n            });\n        }\n    }\n\n    showSubscribe(obj,id){\n        this.subscribeProfile = obj;\n    }\n}\n","import { Component, OnInit } from '@angular/core';\nimport { ActivatedRoute, Router } from '@angular/router';\nimport { ProfileService } from 'src/app/services/profile.service';\nimport { IProfile, IPost } from 'src/app/types/profile';\nimport { Subscription } from 'rxjs';\nimport { Constants } from 'src/app/shared/constants/constants';\nimport { NotifyService } from 'src/app/services/notify.service';\nimport { PostService } from 'src/app/services/post.service';\nimport { finalize } from 'rxjs/operators';\nimport { FormBuilder, FormGroup, Validators } from '@angular/forms';\nimport { SocketService } from 'src/app/services/socket.services';\nimport { DisabledTimeFn, DisabledTimePartial } from 'ng-zorro-antd/date-picker';\nimport differenceInCalendarDays from 'date-fns/differenceInCalendarDays';\nimport { PrintScreens } from 'src/app/helpers/printScreen';\nimport { OffersService } from 'src/app/services/offers.service';\nimport { SubscriptionService } from 'src/app/services/subscription.service';\n\n@Component({\n    selector: 'app-viewprofile',\n    templateUrl: './view-profile.component.html',\n    styleUrls: ['./view-profile.component.less'],\n})\nexport class ViewProfileComponent implements OnInit {\n\n    //Renew user\n\n    handleOkSubscribe(): void {\n        this.isVisibleSubscribe = false;\n    }\n\n    handleCancelSubscribe(): void {\n        this.selectedIndex = null;\n        this.isVisibleSubscribe = false;\n    }\n    ccode: any;\n    selectMenu: string = 'all';\n    public url = Constants.baseUrl2;\n    subscribeValue = 'A';\n    subscribeSelectPayment = 'wallet';\n    coupon_per: any = 0;\n    coupon_id: any;\n    payPrice: any = 0;\n    errmessage: any = ' ';\n    subscription_price = 0;\n    dis_amount: any = 0;\n    selectedIndex: any;\n    selcteduser: any = {};\n    selectedUserOnTimeCoupon: any = {};\n    isVisibleSubscribe = false;\n    renew_id: any;\n    listOfData = [];\n    isblocked: any;\n    isLoading = false;\n    // Modal -- Subscribe -- selected view\n    subscribe = 'subscribeSelect';\n    // Profile View Modal\n    userPreviewPop = false;\n    popUpImg = '';\n    userImgPreview(img) {\n        this.userPreviewPop = true;\n        this.popUpImg = img;\n    }\n    chatPreviewClose() {\n        this.userPreviewPop = false;\n        this.popUpImg = '';\n    }\n    // Cover View Modal\n    coverPreviewPop = false;\n    coverpopUpImg = '';\n    coverImgPreview(img) {\n        this.coverPreviewPop = true;\n        this.coverpopUpImg = img;\n    }\n    coverPreviewClose() {\n        this.coverPreviewPop = false;\n        this.coverpopUpImg = '';\n    }\n\n    // Skeleton Loader -- Profile Loading\n    profileLoading = true;\n    // Modal -- Rate user\n    isVisibleRateUser = false;\n\n    appUrl = Constants.baseUrl2;\n    uiUrl = Constants.app;\n\n    rate = 0;\n    today = new Date();\n    // Modal -- Get Premium\n    isVisiblePremium = false;\n\n    // Modal -- Favourite\n    isVisibleFavourite = false;\n\n    // Modal -- Unsubscribe\n    isVisibleUnsubscribe = false;\n    showModalUnsubscribe(): void {\n        this.isVisibleUnsubscribe = true;\n    }\n\n    handleOkUnsubscribe(): void {\n        this.isVisibleUnsubscribe = false;\n    }\n\n    handleCancelUnsubscribe(): void {\n        this.isVisibleUnsubscribe = false;\n    }\n\n\n    radioValue = 'A';\n    userName = '';\n    profile: IProfile;\n    userProfile: IProfile;\n    subscribeProfile: IProfile;\n    loadingStatus = 2;\n    subscriptions: Subscription[] = [];\n    isFollowing = false;\n    user_id: any;\n    posts: IPost[] = [];\n    totalPosts = 0;\n    fetching = false;\n    limit = 5;\n    isBlocked = false;\n    availableSlots = '8';\n    callDuration: any = {};\n    callSchedule = 'callCreate';\n    callTimings = [];\n    validateForm!: FormGroup;\n    callSchedulePayment = 'wallet';\n    trial_status = 0;\n    imgcount: any = {};\n    imageCount: any;\n    userIsBlocked = false;\n    isExpiry = false;\n    myProfile;\n    likeLength = 0;\n    isFollow = false;\n    // Modal -- Call Schedule\n    isVisibleCallSchedule = false;\n    showModalCallSchedule(): void {\n        this.callSchedule = 'callCreate';\n        this.isVisibleCallSchedule = true;\n        this.availableSlots = null;\n        this.callDuration = null;\n    }\n\n    constructor(\n        private route: ActivatedRoute,\n        private router: Router,\n        private fb: FormBuilder,\n        private profileService: ProfileService,\n        private postService: PostService,\n        private socketService: SocketService,\n        private notify: NotifyService,\n        private offersservice: OffersService,\n        private subscriptionService: SubscriptionService\n    ) { }\n\n    ngOnInit(): void {\n      \n             this.GetProfile();\n            let printSrc = new PrintScreens();\n            printSrc.PrintScr();\n            this.validateForm = this.fb.group({\n                userName: [null, [Validators.required]],\n                callDuration: [null, [Validators.required]],\n            });\n            this.route.params.subscribe(routeParams => {\n                this.userName = routeParams.id;\n                if (localStorage.getItem('display_name') == this.userName || localStorage.getItem('user_name') == this.userName) {\n                    this.router.navigate(['/profile'])\n                }\n                else{\n                    setTimeout(() => {\n                        this.GetUserProfile();\n                    }, 2000);\n                }\n               \n            });\n      \n    }\n\n    Message(email) {\n        localStorage.setItem(\n            'message_email',\n            email\n        );\n        this.router.navigate(['/messages']);\n    }\n\n    ngOnDestroy() {\n        this.subscriptions.forEach((item) => item.unsubscribe());\n    }\n\n    GetProfile() {\n        this.subscriptions.push(\n            this.profileService.GetProfileSub().subscribe((data) => {\n                if (data && data['success']) {\n                    this.profile = data;\n                    if (this.profileService.isMyUserName(this.userName))\n                        return this.router.navigate(['/profile']);\n                    this.profile = data;\n                    this.user_id = this.profile['_id'];\n                    this.trial_status = data.trial_status;\n                }\n            })\n        );\n    }\n\n    GetFavList(val) {\n        this.profileService.GetCount(val).subscribe((res) => {\n            this.imgcount = res;\n        });\n    }\n\n    handleCancelCallSchedule(): void {\n        this.isVisibleCallSchedule = false;\n        this.callSchedule = 'callCreate';\n    }\n\n    submitForm(): void {\n        for (const i in this.validateForm.controls) {\n            this.validateForm.controls[i].markAsDirty();\n            this.validateForm.controls[i].updateValueAndValidity();\n        }\n    }\n\n    disabledDate = (current: Date): boolean => {\n        return differenceInCalendarDays(current, this.today) < 0;\n    };\n    range(start: number, end: number): number[] {\n        const result: number[] = [];\n        for (let i = start; i < end; i++) {\n            result.push(i);\n        }\n        return result;\n    }\n    disabledDateTime: DisabledTimeFn = () => {\n        let x = new Date();\n        let hour = 0;\n        let minutes = 0;\n        let seconds = x.getSeconds();\n        return {\n            nzDisabledHours: () => this.range(0, hour),\n            nzDisabledMinutes: () => this.range(0, minutes),\n            nzDisabledSeconds: () => this.range(0, 0),\n        };\n    };\n\n    getlike(id) {\n        this.likeLength = id;\n    }\n\n    GetUserProfile() {\n        this.profileService.GetUserProfileByName(this.userName).subscribe(\n            (data) => {\n                if (data['data']) {\n                    this.userProfile = data['data'];\n                    let user = [];\n                    let image = [];\n                    user = data['data']['posts'];\n                    let userid = this.userProfile['_id'];\n\n                    for (let i = 0; i < user.length; i++) {\n                        this.likeLength = user[i].likes.length;\n                    }\n\n                    if (this.userProfile.followers.some(function (el) { return el.user_id == localStorage.getItem('user_id') && el.blocked == true && el.blocked_by == userid })\n                        || this.userProfile.following.some(function (el) { return el.follower_id == localStorage.getItem('user_id') && el.blocked == true && el.blocked_by == userid })) {\n                        this.userIsBlocked = true;\n                    }\n                    else {\n                        this.userIsBlocked = false;\n                    }\n                    for (let i = 0; i < user.length; i++) {\n                        for (let j = 0; j < user[i].file.length; j++) {\n                            if (\n                                user[i].file[j]['type'] == 'image/png' ||\n                                user[i].file[j]['type'] == 'image/jpg' ||\n                                user[i].file[j]['type'] == 'image/svg'\n                            ) {\n                                image.push(user[i].file[j]);\n                            }\n                        }\n                    }\n                    this.imageCount = image.length;\n                    this.GetFavList(this.userProfile._id);\n                    this.isBlocked = data['isBlocked'];\n                    this.subscribeProfile = data['data'];\n                    if (data['follower'][0] == 1) this.isFollowing = true;\n                    this.loadingStatus = 1;\n                    this.callTimings = data['data'].calltimings;\n                } else {\n                    this.loadingStatus = 0;\n                }\n                if (this.profile.following.some(function (el) { return el.follower_id == data.data._id && el.plan_status == 0 })) {\n                    this.isExpiry = true;\n                }\n                if (this.userProfile.follows.some(function (el) { return el.user_id == localStorage.getItem('user_id') })) {\n                    this.isFollow = true;\n                }\n                else {\n                    this.isFollow = false;\n                }\n                setTimeout(() => {\n                    this.profileLoading = false;\n                }, 1200);\n            },\n            (err) => {\n                this.loadingStatus = 0;\n            }\n        );\n    }\n\n    ReportUser() {\n        this.profileService\n            .ReportUser({ reportuser_id: this.userProfile._id })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    BlockUser() {\n        this.profileService\n            .BlockUser({ block_id: this.userProfile._id })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.isBlocked = true;\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    UnBlockUser() {\n        this.profileService\n            .UnblockUser({ block_id: this.userProfile._id })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.isBlocked = false;\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    GetUserPosts() {\n        if (\n            !this.fetching &&\n            (this.totalPosts == 0 || this.totalPosts > this.posts.length)\n        ) {\n\n        }\n    }\n\n    RateUser(content) {\n        this.profileService\n            .RateUser({\n                follower_id: this.userProfile._id,\n                rating: this.rate,\n                content,\n            })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.isVisibleRateUser = false;\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    ScheduleCall() {\n        this.isLoading = true;\n        let val = {\n            caller_id: this.userProfile._id,\n            requestedtime: new Date(this.validateForm.value.userName).getTime(),\n            availabletime: new Date(this.validateForm.value.userName).getTime(),\n            amount: this.callDuration.price,\n            duration: this.callDuration.duration,\n        };\n        this.socketService.ScheduleCall(val).subscribe((res) => {\n            if (res['success']) {\n                this.notify.success(res['message']);\n                this.isLoading = false;\n\n                this.profileService.TriggerProfile();\n                 this.handleCancelCallSchedule();\n\n            } else {\n                this.notify.error(res['message']);\n                this.isLoading = false;\n\n            }\n        },\n            (err) => {\n                this.notify.error(err.error['message']);\n                this.isLoading = false;\n\n            });\n    }\n\n    TrialSubscribeUser(usr) {\n        let val = {\n            follower_id: usr._id,\n        };\n        this.profileService.SubscribeUser(val).subscribe((res) => {\n            if (res['success']) {\n                this.notify.success(res['message']);\n                location.reload();\n            } else {\n                this.notify.error(res['message']);\n            }\n        });\n    }\n\n    showModalSubscribe(usr, i): void {\n        this.subscribe = 'subscribeSelect'\n        this.renew_id = i;\n        this.selcteduser = usr;\n        this.selectedUserOnTimeCoupon = usr;\n        this.isVisibleSubscribe = true;\n    }\n\n    getsubscribeValue() {\n        if (this.subscribeValue == 'A') {\n            this.subscription_price = this.selcteduser.subscription_price;\n        } else if (this.subscribeValue == 'B') {\n            this.subscription_price = this.selcteduser.subscription_price_6m;\n        } else if (this.subscribeValue == 'C') {\n            this.subscription_price = this.selcteduser.subscription_price_1yr;\n        }\n        else if (this.subscribeValue == 'D') {\n            this.subscription_price = this.selcteduser.subscription_price_3m;\n        } else {\n            this.subscription_price = this.selcteduser.subscription_price;\n        }\n    }\n\n    getCouponList() {\n        this.offersservice.GetAvaiCoupons().subscribe((data) => {\n            if (data['success']) {\n                this.listOfData = data['data'];\n                if (this.subscribeValue == 'A') {\n                    this.payPrice = this.selcteduser.subscription_price;\n                } else if (this.subscribeValue == 'B') {\n                    this.payPrice = this.selcteduser.subscription_price_6m;\n                } else if (this.subscribeValue == 'C') {\n                    this.payPrice = this.selcteduser.subscription_price_1yr;\n                }\n                else if (this.subscribeValue == 'D') {\n                    this.payPrice = this.selcteduser.subscription_price_3m;\n                }\n                let x = this.listOfData.find(\n                    (ob) => ob.couponcode == this.ccode\n                );\n                if (x) {\n                    this.coupon_id = x._id;\n                    this.coupon_per = x.percentage;\n                    this.errmessage = '';\n                    this.dis_amount = (this.payPrice * x.percentage) / 100;\n                } else {\n                    this.dis_amount = 0;\n                    this.errmessage = 'NO such Coupon Found';\n                }\n            }\n        });\n    }\n\n    ExtendUSer(i) {\n        // console.log(this.subscription_price);\n        let paymentprice = 0;\n        let paymentmonth = 0;\n        this.payPrice = this.subscription_price;\n        if (this.subscribeValue == 'A') {\n            paymentmonth = 1;\n        } else if (this.subscribeValue == 'B') {\n            paymentmonth = 6;\n        } else if (this.subscribeValue == 'C') {\n            paymentmonth = 12;\n        }\n        else if (this.subscribeValue == 'D') {\n            paymentmonth = 3;\n        }\n        let val = {\n            follower_id: this.selcteduser._id,\n            months: paymentmonth,\n            amount: this.payPrice,\n            coupon_code: this.ccode,\n            coupon_id: this.coupon_id,\n        };\n        this.profileService.RenewSubscribeUser(val).subscribe((res) => {\n            if (res['success']) {\n                this.notify.success(res['message']);\n                this.selectMenu = 'all';\n                this.handleCancelSubscribe();\n                location.reload();\n            } else {\n                this.subscription_price = 0;\n                this.notify.error(res['message']);\n            }\n        });\n    }\n\n    UnSubUser(val) {\n        let f_id = {\n            follower_id: val\n        }\n        this.profileService.UnsubscribeUser(f_id).subscribe((res) => {\n            if (res['success']) {\n                this.notify.success(res['message']);\n                this.handleCancelUnsubscribe();\n                location.reload();\n            }\n            else {\n                this.notify.success(res['message'])\n            }\n        })\n    }\n\n    followUser(obj) {\n        let val = {\n            follow_id: obj\n        }\n        this.subscriptionService.FollowUser(val).subscribe((res) => {\n            if (res['success']) {\n                this.notify.success(res['message']);\n                this.profileService.TriggerProfile();\n                this.GetUserProfile();\n            }\n            else {\n                this.notify.error(res['message'])\n            }\n        },\n            (err) => {\n                this.notify.error(err.error['message'])\n            })\n    }\n\n    unfollowUser(obj) {\n        let val = {\n            follow_id: obj\n        }\n        this.subscriptionService.UnFollowUser(val).subscribe((res) => {\n            if (res['success']) {\n                this.notify.success(res['message']);\n                this.profileService.TriggerProfile();\n                this.GetUserProfile();\n            }\n            else {\n                this.notify.error(res['message'])\n            }\n        },\n            (err) => {\n                this.notify.error(err.error['message'])\n            })\n    }\n}\n","<div class=\"main-layout\">\n    <div *ngIf=\"userProfile?.blocked\">\n        <div class=\"layout-inner-body d-flex justify-content-center\">\n            <nz-result nzStatus=\"info\" nzTitle=\"User blocked\">\n                <div nz-result-extra>\n                    {{\"AdminBlock\" | translate}}\n                </div>\n            </nz-result>\n        </div>\n    </div>\n    <div *ngIf=\"userIsBlocked\">\n        <div class=\"layout-inner-body d-flex justify-content-center\">\n            <nz-result nzStatus=\"info\" nzTitle=\"User blocked\">\n                <div nz-result-extra>\n                    {{\"userBlock\" | translate}}\n                </div>\n            </nz-result>\n        </div>\n    </div>\n    <div class=\"layout-body\" *ngIf=\"!userProfile?.blocked && !userIsBlocked\">\n        <nz-spin *ngIf=\"loadingStatus == 2\"></nz-spin>\n        <div class=\"layout-inner-body\" *ngIf=\"loadingStatus == 1\">\n            <!-- Layout Feed --start -->\n            <div class=\"layout-feed\">\n                <div class=\"position-sticky\">\n                    <div class=\"page-header\">\n                        <a routerLink=\"/home\" class=\"page-prev-btn\">\n                            <i-feather name=\"arrow-left\"></i-feather>\n                        </a>\n                    