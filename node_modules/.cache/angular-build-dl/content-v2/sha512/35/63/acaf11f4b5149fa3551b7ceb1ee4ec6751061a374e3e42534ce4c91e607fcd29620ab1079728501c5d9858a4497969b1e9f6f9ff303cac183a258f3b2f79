blocked;\n                    this.translate.use(validLanguage(data['language']));\n          \n                    if (data && data['bank_info']) {\n                        this.ifsc = this.profile['bank_info']['ifsc_code'];\n                        this.accno = this.profile['bank_info']['acc_number'];\n                    }\n                    this.kycStatus = this.profile['kyc_status'];\n                    this.balance = this.profile['balance'];\n                }\n            })\n        );\n    }\n    createWithdraw() {\n      if (this.ifsc == null) {\n            this.notify.error('<a href=\"/addbank\">Add your bank Info </a>');\n        }\n        else if (this.amount.value == 0 || this.balance == 0) {\n            this.notify.error(this.translate.instant('Insufficient'));\n        }  else if (this.kycStatus != '1') {\n            this.notify.error(this.translate.instant('KYCPending'));\n        } else {\n            let val = {\n                amount: parseFloat(this.amount.value),\n                ifsc: this.profile.bank_info.ifsc_code,\n                accountno: this.profile.bank_info.acc_number,\n                bank: this.profile.bank_info.bank_name,\n                crypto: 'false',\n            };\n            this.withdrawService.CreateWithdraw(val).subscribe(\n                (res) => {\n                    this.notify.success(res['message']);\n                    this.profileService.TriggerProfile();\n                      this.amount.reset();\n                      this.ngOnInit();\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n        }\n    }\n\n    resetAmount(){\n        this.amount.reset();\n    }\n}\n","<div class=\"main-layout\">\n    <div *ngIf=\"isblocked\">\n        <div class=\"layout-inner-body d-flex justify-content-center\">\n            <nz-result nzStatus=\"info\" nzTitle=\"User blocked\">\n                <div nz-result-extra>\n  