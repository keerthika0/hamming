           });\n                });\n            });\n\n    }\n\n    userone:any;\n    cid:any;\n    myvideo:any;\n    addVideoStream(video, stream) {\n\n        video.srcObject = stream;\n        video.addEventListener('loadedmetadata', () => {\n            video.play();\n        });\n        // this.myvideo = video;\n        this.connectingStatus.next({status:true});\n        this.cid = document.getElementById('video-grid').append(video);\n    }\n\n    connectToNewUser(userId, stream) {\n       \n        const call = this.myPeer.call(userId, stream);\n        const video = document.createElement('video');\n        call.on('stream', (userVideoStream) => {\n            this.addVideoStream(video, userVideoStream);\n        });\n        call.on('close', () => {\n            video.remove();\n        });\n\n        this.peers[userId] = call;\n    }\n}\n","import { Injectable } from '@angular/core';\nimport { HttpService } from './http.service';\nimport { Constants } from '../shared/constants/constants';\n\n@Injectable({\n  providedIn: 'root'\n})\nexport class ListService {\n\n  constructor(private http: HttpService) { }\n  \n  GetFollower() {\n   return this.http.get(Constants.url + Constants.getFollower);\n}\n\nGetAllFollowing() {\n  return this.http.get(Constants.url + Constants.getallFollowing);\n}\nGetAllFollowers() {\n  return this.http.get(Constants.url + Constants.getallFollower);\n}\n\nGetFollowing() {\n  return this.http.get(Constants.url + Constants.getFollowing);\n}\n\nGetexpiryFollowing() {\n  return this.http.get(Constants.url + Constants.getexpiryFollowing);\n}\n\nGetexpiryFollower() {\n  return this.http.get(Constants.url + Constants.getexpiryFollower);\n}\n\nGetScreenList() {\n  return this.http.get(Constants.url + Constants.getScreenList);\n}\n\nGetFav(val){\n  return this.http.get(Constants.url + Constants.getFavourites + val);\n}\n\n//FAQ\nFaqLsit() {\n  return this.http.get(Constants.url + Constants.faqlist);\n}\n\n// RemovefromList(){\n//   return t