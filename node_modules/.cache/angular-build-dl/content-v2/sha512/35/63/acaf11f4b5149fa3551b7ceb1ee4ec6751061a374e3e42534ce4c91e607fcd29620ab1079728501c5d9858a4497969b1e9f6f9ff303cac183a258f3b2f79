ifications\" | translate}}\n                        </div>\n                        <div>\n                            <i-feather name=\"chevron-right\"></i-feather>\n                        </div>\n                        <div class=\"active-item\" *ngIf=\"tab == 5\"></div>\n                    </a>\n                    <!-- <a class=\"edit-item\" (click)=\"tab = 6\">\n                        <div class=\"title\">\n                            {{\"Languages\" | translate}}\n                        </div>\n                        <div>\n                            <i-feather name=\"chevron-right\"></i-feather>\n                        </div>\n                        <div class=\"active-item\" *ngIf=\"tab == 6\"></div>\n                    </a> -->\n                </div>\n            </div>\n            <!-- Edit Layout -- End -->\n            <!-- Edit Layout Content -- Start -->\n            <!-- Edit Profile -- Start -->\n\n            <!-- Edit Profile -- End -->\n            <!-- Account -- Start -->\n            <div class=\"edit-layout-content\" *ngIf=\"tab == 1\">\n                <div class=\"page-header justify-content-between\">\n                    <div class=\"page-title\"> {{\"Account\" | translate}}</div>\n                </div>\n                <div class=\"edit-form\">\n                    <form nz-form [formGroup]=\"accountForm\" (ngSubmit)=\"UpdateAccount()\">\n                        <nz-form-item>\n                            <nz-form-label nzFor=\"email\">{{\"EmailId\" | translate}}</nz-form-label>\n                            <nz-form-control [nzErrorTip]=\"emailErrorTpl\">\n                                <input nz-input name=\"email\" type=\"email\" [(ngModel)]=\"email\" readonly\n                                    style=\"cursor: no-drop !important;\" id=\"email\" formControlName=\"email\"\n                                    placeholder=\"johndoe@gmail.com\" />\n                                <ng-template #emailErrorTpl let-control>\n    