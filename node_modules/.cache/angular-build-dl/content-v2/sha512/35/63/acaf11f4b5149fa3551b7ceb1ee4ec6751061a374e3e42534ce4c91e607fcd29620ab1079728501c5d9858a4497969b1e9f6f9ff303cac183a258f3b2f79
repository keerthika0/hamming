Route } from '@angular/router';\nimport { DisabledTimeFn, DisabledTimePartial } from 'ng-zorro-antd/date-picker';\nimport { ProfileService } from 'src/app/services/profile.service';\nimport { validLanguage } from 'src/app/helpers/languages';\nimport { TranslateService } from '@ngx-translate/core';\n\n\n@Component({\n    selector: 'app-messages',\n    templateUrl: './messages.component.html',\n    styleUrls: ['./messages.component.less'],\n})\nexport class MessagesComponent implements OnInit {\n    // Chat Modal\n    chatPreviewPop = false;\n    popUpImg = '';\n    chatImgPreview(img) {\n        this.chatPreviewPop = true;\n        this.popUpImg = img;\n    }\n\n    chatPreviewClose() {\n        this.chatPreviewPop = false;\n        this.popUpImg = '';\n    }\n\n    appUrl = Constants.baseUrl2;\n    subscriptions: Subscription[] = [];\n    today = new Date();\n    tipsPayment = 'tipsAmount';\n    msgLists: any = [];\n    msgListsTemp: any = [];\n    userMessage: any;\n    chatUser: any = {};\n    User: any = {};\n    selectedIndex = 0;\n    selectedChat: any;\n    userTyping: Boolean = false;\n    newChat: Boolean = false;\n    messageText: any = '';\n    callTimings: any = [];\n    messagesent: false;\n    followersList: any = [];\n    file: any;\n    isExpiry = 'false';\n    myEmail = localStorage.getItem('email');\n    imageSrc;\n    format;\n    availableSlots = '8';\n    callDuration: any = {};\n    tipsSelectPayment = 'wallet';\n    callSchedulePayment = 'wallet';\n    callSchedule = 'callCreate';\n    validateForm!: FormGroup;\n    isVisibleTips = false;\n    inputValue = '';\n    searchTextChanged = new Subject<string>();\n    searching = false;\n    current_chat_id = 0;\n    messageCount = 0;\n    isblocked;\n    isShow = false;\n    // Modal -- Delete Chat\n    isVisibleDeleteChat = false;\n    showModalDeleteChat(): void {\n        this.isVisibleDeleteChat = true;\n    }\n\n    handleOkDeleteChat(): void {\n        this.isVisibleDeleteChat = false;\n    }\n\n    handleCancelDeleteChat(): void {\n        this.isVisibleDeleteChat = false;\n    }\n\n    // Modal -- Call Schedule\n    isVisibleCallSchedule = false;\n    showModalCallSchedule(id): void {\n          if (this.User.followers.some(function (el) { return el.user_id == id && el.blocked == true })\n            || this.User.following.some(function (el) { return el.follower_id == id && el.blocked == true })) {\n            this.notify.error(this.translate.instant('Blocked'))\n        }\n        else if (this.User.following.some(function (el) { return el.follower_id === id && el.plan_status === 0 })) {\n          this.notify.error(this.translate.instant('Renew User'))\n        }\n        else {\n            this.callSchedule = 'callCreate';\n            this.isVisibleCallSchedule = true;\n            this.availableSlots = null;\n            this.callDuration = null;\n        }\n    }\n\n    handleOkCallSchedulet(): void {\n        this.isVisibleCallSchedule = false;\n    }\n\n    handleCancelCallSchedule(): void {\n        this.isVisibleCallSchedule = false;\n        this.callSchedule = 'callCreate';\n    }\n\n    submitForm(): void {\n        for (const i in this.validateForm.controls) {\n            this.validateForm.controls[i].markAsDirty();\n            this.validateForm.controls[i].updateValueAndValidity();\n        }\n    }\n\n    constructor(\n        private message: NzMessageService,\n        private sanitizer: DomSanitizer,\n        private translate: TranslateService,\n        private fb: FormBuilder,\n        private socketService: SocketService,\n        private messageService: MessageService,\n        private notify: NotifyService,\n        private cdref: ChangeDetectorRef,\n        private profileService: ProfileService,\n    ) { }\n\n    currentmessage;\n\n    ngOnInit(): void {\n        this.MessageTyping();\n        this.GetFollowers();\n        this.profileService.TriggerProfile();\n        this.GetMyProfile();\n        this.validateForm = this.fb.group({\n            userName: [null, [Validators.required]],\n            callDuration: [null, [Validators.required]],\n        });\n\n        this.SearchFunction();\n        this.GetMsgList();\n        this.MessageSentStatus();\n        this.NewMessage();\n    }\n\n    GetMyProfile() {\n        this.subscriptions.push(\n            this.profileService.GetProfileSub().subscribe((data) => {\n                if (data && data['success']) {\n                    this.User = data;\n                    this.translate.use(validLanguage(data['language']));\n                    this.isblocked = this.User.blocked;\n                }\n            })\n        );\n    }\n\n    ngAfterContentChecked() {\n        this.cdref.detectChanges();\n    }\n\n    ngAfterViewChecked() {\n        this.cdref.detectChanges();\n    }\n\n    GetFollowers() {\n        this.socketService.GetFollowers().subscribe((res) => {\n            // console.log(res);\n            this.followersList = res['data'];\n\n        });\n    }\n\n    chatuserpro = false;\n    chatuserfoll = false;\n    // array_move(arr, old_index, new_index) {\n    //     if (new_index >= arr.length) {\n    //         var k = new_index - arr.length + 1;\n    //         while (k--) {\n    //             arr.push(undefined);\n    //         }\n    //     }\n    //     arr.splice(new_index, 0, arr.splice(old_index, 1)[0]);\n    //     return arr; // for testing\n    // };\n\n    public async GetMsgList() {\n        this.socketService.GetMsgList().subscribe((res) => {\n            this.msgLists = res['msgs'];\n\n            if (res['msgs'] && this.msgLists.length > 0) {\n                this.isShow = true;\n                this.msgListsTemp = this.msgLists;\n                if (localStorage.getItem('message_email')) {\n                    this.messageCount = 0;\n                    for (let i = 0; i < this.msgLists.length; i++) {\n                        this.msgLists[i].new = this.msgLists[i].unread_messages;\n                        this.messageCount += this.msgLists[i].new;\n\n                        if (this.msgLists[i].users[0] == localStorage.getItem('message_email') || this.msgLists[i].users[1] == localStorage.getItem('message_email')) {\n                            this.GetMessages(this.msgLists[i]);\n                            // this.array_move(this.msgLists, i, 0)\n                            // this.selectedIndex = i;\n                            this.chatuserpro = true\n\n                        }\n                    }\n\n                    localStorage.setItem(\"message_count\", (this.messageCount).toString())\n                    this.messageService.reduceCount(1);\n\n                    if (this.chatuserpro == false) {\n                        this.socketService.GetFollowers().subscribe((res) => {\n                            this.followersList = res['data'];\n                            for (let foll of this.followersList) {\n                                if (foll.user_id.email == localStorage.getItem('message_email')) {\n                                    this.NewChatmessage(foll.user_id);\n                                    this.chatuserfoll = true;\n                                }\n                            }\n                            if (this.chatuserfoll == false) {\n                                this.GetMessages(this.msgLists[0]);\n                            }\n                        });\n                    }\n                } else {\n                    this.GetMessages(this.msgLists[0]);\n                }\n            } else {\n            }\n        });\n    }\n\n    deleteChat() {\n        if (this.current_chat_id != 0) {\n            this.messageService.DeleteChat(this.current_chat_id).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        if (data['success']) {\n                            this.isVisibleDeleteChat = false;\n\n                            this.msgLists.splice(this.selectedIndex, 1);\n                            if (this.msgLists.length > 0) {\n                                this.msgListsTemp = this.msgLists;\n                                this.isShow = true;\n                                this.GetMessages(this.msgLists[0]);\n                            } else {\n                                this.chatUser = {};\n                                this.isShow = false;\n                            }\n\n                            this.current_chat_id = 0;\n                            this.notify.success(data['message']);\n                            location.reload();\n\n                        }\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        }\n    }\n\n    readAllChat() {\n        if (this.current_chat_id != 0) {\n            this.messageService.ReadAllChat(this.current_chat_id).subscribe(\n                (data) => {\n\n                }\n            );\n        }\n    }\n\n    NewChat() {\n        this.newChat = true;\n    }\n\n    NewChatmessage(user) {\n        this.chatUser = user;\n        this.isShow = true;\n        this.userMessage = [];\n        this.selectedChat = {};\n        this.callTimings = [];\n\n        if (this.chatUser.calltimings && this.chatUser.calltimings.length > 0) {\n            this.callTimings = this.chatUser.calltimings;\n        }\n\n        //When you click New Message Menu and if same use have old mesaages to fetch it\n\n        for (let cht of this.msgLists) {\n            if (cht.chatUser[0] && cht.chatUser[0].email == user.email) {\n                for (var i = 0; i < cht.messages.length; i++) {\n                    if (cht.messages[i].deleted && this.myEmail == cht.messages[i].deleted[0] || this.myEmail == cht.messages[i].deleted[1]) {\n                        cht.messages[i].isdeleted = true;\n                    } else {\n                        cht.messages[i].isdeleted = false;\n                    }\n                }\n\n                this.userMessage = cht.messages;\n            }\n        }\n    }\n\n    MessageTyping() {\n        this.socketService.MessageTyping().subscribe((res) => {\n            if (res.from == this.chatUser.email) {\n                this.userTyping = true;\n                setTimeout(() => {\n                    this.userTyping = false;\n                }, 1000);\n            }\n        });\n    }\n\n    SearchFunction() {\n        this.subscriptions.push(\n            this.searchTextChanged.pipe(debounceTime(300)).subscribe((val) => {\n                this.searching = true;\n                if (val) {\n                    this.msgListsTemp = this.msgLists.filter((item) => {\n                        if (item.chatUser[0]) {\n                            return (\n                                item.chatUser[0].user_name\n                                    .toLowerCase()\n                                    .includes(val.toLowerCase()) ||\n                                item.chatUser[0].display_name\n                                    .toLowerCase()\n                                    .includes(val.toLowerCase()) ||\n                                item.chatUser[0].email\n                                    .toLowerCase()\n                                    .includes(val.toLowerCase())\n                            );\n                        }\n                    });\n                } else {\n                    this.msgListsTemp = this.msgLists;\n\n                }\n                this.searching = false;\n            })\n        );\n    }\n\n    Search(val) {\n        this.searchTextChanged.next(val);\n    }\n\n    disabledDate = (current: Date): boolean => {\n        return differenceInCalendarDays(current, this.today) < 0;\n    };\n    range(start: number, end: number): number[] {\n        const result: number[] = [];\n        for (let i = start; i < end; i++) {\n            result.push(i);\n        }\n        return result;\n    }\n    disabledDateTime: DisabledTimeFn = () => {\n        let x = new Date();\n        let hour = 0;\n        let minutes = 0;\n        let seconds = x.getSeconds();\n        return {\n            nzDisabledHours: () => this.range(0, hour),\n            nzDisabledMinutes: () => this.range(0, minutes),\n            nzDisabledSeconds: () => this.range(0, 0)\n        };\n    };\n\n\n    task() {\n        // console.log(this.callDuration, this.availableSlots, this.validateForm.value.userName)\n    }\n\n    NewMessage() {\n        this.socketService.NewMessage().subscribe((res) => {\n            if (res && res.chatId) {\n                for (let cht of this.msgLists) {\n                    if (cht.chatId == res.chatId) {\n                        cht.new += 1;\n                        if (res.data && res.data.length > 0) {\n                            if (cht.messages[cht.messages.length - 1]._id !== res.data[0]._id) {\n                                cht.messages.push(res.data[0]);\n                            }\n                        } else if (res.data) {\n                            if (cht.messages[cht.messages.length - 1]._id !== res.data._id) {\n                                cht.messages.push(res.data);\n                            }\n                        } else {\n                            cht.messages.push(res);\n                        }\n                    }\n                }\n            }\n        });\n    }\n\n    MessageSentStatus() {\n        // console.log('msg msg')\n        this.socketService.MsgSentStatus().subscribe((res) => {\n            this.messagesent = res;\n            // console.log(res);\n            if (res && res.chatId) {\n                for (let cht of this.msgLists) {\n                    if (cht.chatId == res.chatId) {\n                        if (res.data && res.data.length > 0) {\n                            cht.messages.push(res.data[0]);\n                        } else if (res.data) {\n                            cht.messages.push(res.data);\n                        } else {\n                            cht.messages.push(res);\n                        }\n                        this.messageText = '';\n                    }\n                }\n            }\n            // else if (res && !res.chatId) {\n            //     // for (let cht of this.msgLists) {\n            //     // console.log(res.to);\n            //     // if (res.to == cht.users[0] || res.to == cht.users[1])\n            //     //     cht.messages.push(res);\n            //     // }\n            // }\n        });\n    }\n    MessCount(i) {\n\n        localStorage.setItem(\"message_count\", (Number(localStorage.getItem('message_count')) - this.msgListsTemp[i].new).toString())\n        this.messageService.reduceCount(this.msgListsTemp[i].new);\n\n        this.msgListsTemp[i].new = 0;\n    }\n\n    GetMessages(messages) {\n        this.current_chat_id = messages._id;\n        this.callTimings = [];\n        this.selectedChat = messages;\n        this.chatUser = messages.chatUser[0];\n        this.readAllChat();\n        if (messages.chatUser[0]) {\n            this.callTimings = this.chatUser.calltimings;\n        }\n\n        for (var j = 0; j < messages.messages.length; j++) {\n            messages.messages[j].isdeleted = false;\n            if (messages.messages[j].deleted) {\n                for (var k = 0; k < messages.messages[j].deleted.length; k++) {\n                    if (messages.messages[j].deleted[k] == this.myEmail) {\n                        messages.messages[j].isdeleted = true;\n                    }\n                }\n            }\n        }\n\n        this.userMessage = messages.messages;\n        // this.selectedIndex = i;\n\n    }\n\n    public sendMessage(id) {\n        if (this.User.followers.some(function (el) { return el.user_id == id && el.blocked == true })\n            || this.User.following.some(function (el) { return el.follower_id == id && el.blocked == true })) {\n            this.notify.error(this.translate.instant('Blocked'))\n        }\n        else if (this.User.following.some(function (el) { return el.follower_id == id && el.plan_status == 0 })) {\n            this.notify.error(this.translate.instant('Renew User'))\n        }\n        else {\n            if (this.file && this.imageSrc) {\n                this.SendFile();\n            } else {\n                if (this.messageText)\n                    this.socketService.SendMessage({\n                        to: this.chatUser.email,\n                        msg: this.messageText,\n                        chatId: this.selectedChat.chatId,\n                    });\n                localStorage.setItem('message_email', this.messageText)\n            }\n            this.messageText = '';\n            if (this.newChat) {\n                this.newChat = false;\n                this.searching = false;\n\n                this.GetMsgList();\n            }\n        }\n    }\n\n    public Typing(event) {\n        this.socketService.Typing({ to: this.chatUser.email });\n    }\n\n    public GetTyping(event) {\n        this.socketService.Typing({ to: this.chatUser.email });\n    }\n\n    public ScheduleCall() {\n        let val = {\n            caller_id: this.chatUser._id,\n            requestedtime: new Date(this.validateForm.value.userName).getTime(),\n            availabletime: new Date(this.validateForm.value.userName).getTime(),\n            amount: this.callDuration.price,\n            duration: this.callDuration.duration,\n        };\n        this.socketService.ScheduleCall(val).subscribe((res) => {\n            // console.log(res);\n            if (res['success']) {\n                this.GetMyProfile();\n                this.message.success(res['message']);\n                this.handleCancelCallSchedule();\n                location.reload();\n            } else {\n                this.message.error(res['message']);\n            }\n        },\n            (err) => {\n                this.notify.error(err.error['message']);\n            });\n    }\n\n    onFileChange(event) {\n        const reader = new FileReader();\n        if (event.target.files && event.target.files.length) {\n            const [file] = event.target.files;\n            reader.readAsDataURL(file);\n            reader.onload = () => {\n                if (file.type.indexOf('image') > -1) {\n                    this.format = 'image';\n                } else if (file.type.indexOf('video') > -1) {\n                    this.format = 'video';\n                }\n\n                this.imageSrc = this.sanitizer.bypassSecurityTrustUrl(\n                    reader.result as string\n                );\n                this.file = event.target.files[0];\n                // this.myForm.patchValue({\n                //     fileSource: reader.result\n                //  });\n            };\n        }\n    }\n\n    addEmoji(e) {\n        this.messageText += e.emoji.native;\n    }\n\n    SendFile() {\n        this.imageSrc = null;\n        let formdata = new FormData();\n        formdata.append('to', this.chatUser.email);\n        formdata.append('chatId', this.selectedChat.chatId);\n        formdata.append('file', this.file);\n        formdata.append('message', this.messageText);\n\n        this.socketService.SendFile(formdata).subscribe((res) => {\n            if (res['success']) {\n                for (let cht of this.msgLists) {\n                    if (cht.chatId == res.chatId) {\n                        cht.messages.push(res.data[0]);\n                    }\n                }\n            }\n            this.cancelFile();\n        });\n    }\n\n    cancelFile() {\n        this.imageSrc = null;\n        this.file = null;\n    }\n\n    showTips(): void {\n        this.isVisibleTips = true;\n    }\n\n    handleOkTips(): void {\n        this.isVisibleTips = false;\n    }\n\n    handleCancelTips(): void {\n        this.isVisibleTips = false;\n    }\n\n    ngOnDestroy(): void {\n        this.subscriptions.forEach((item) => item.unsubscribe());\n    }\n}","import { Component, OnInit } from '@angular/core';\nimport { SocketService } from '../../../../services/socket.services';\nimport { Constants } from 'src/app/shared/constants/constants';\nimport { Router } from '@angular/router';\nimport { NotifyService } from 'src/app/services/notify.service';\n\n@Component({\n    selector: 'app-notifications-all',\n    templateUrl: './notifications-all.component.html',\n    styleUrls: ['./notifications-all.component.less'],\n})\nexport class NotificationsAllComponent implements OnInit {\n    // Skeleton Loader -- Notification\n    notificationLoading = true;\n\n    appUrl = Constants.baseUrl2;\n    notificationList = [];\n    nonotification = false;\n    user_id;\n    user_name = 'Doodem';\n    email = localStorage.getItem('email');\n    display_name = localStorage.getItem('display_name');\n    profile_image = localStorage.getItem('profile_image');\n\n    constructor(private socketService: SocketService, private router: Router, private notify: NotifyService,) { }\n\n    ngOnInit(): void {\n        this.GetAllNotifications();\n        this.socketService.SetCount(0);\n        this.user_id = localStorage.getItem('user_id');\n      \n    }\n\n\n    NaviPage(val, id) {\n        if(id == true){\n            this.notify.error('User blocked by Admin')\n        }\n       else if (val.senderid.followers.some(function (el) { return el.user_id == localStorage.getItem('user_id') && el.blocked == true })\n        || val.senderid.following.some(function (el) { return el.follower_id == localStorage.getItem('user_id') && el.blocked == true})) {\n    \n            this.notify.error('Not Found')\n\n        }\n        else {\n            if (val.type == 'new_coupon_added') {\n                this.router.navigate(['/offers']);\n            }\n            else if (val.type == 'Newcomment' ||\n                val.type == 'commentlike' || val.type == 'replycomment