                this.HistoryonScroll();\n                }\n            }\n        }\n    }\n\n\n    HistoryonScroll() {\n        if (this.count > 0) {\n            if (this.count >= this.limit) {\n                this.count = this.count - this.limit;\n            }\n            if (this.count < this.limit && this.count != 0) {\n                this.limit = this.count;\n                this.count = this.count - this.limit;\n            }\n            if (this.limit != 0) {\n                this.walletservice\n                .GetPaymentHistoryListById(this.id, this.limit)\n                .subscribe(\n                    async (data) => {\n                        if (data['success']) {\n\n                            this.loading = false;\n\n                            for (var i = 0; i < data['data'].length; i++) {\n                                this.paymentHistory.push(data['data'][i]);\n                            }\n                            this.id = data['data'].length - 1;\n                            this.id = data['data'][this.id]._id;\n                            this.datahited = false;\n                        }\n                    },\n                    (err) => {\n                        this.notify.error(err.error.message);\n                    }\n                    );\n            }else{\n                this.loading = false;\n            }\n        }else{\n           this.loading = false; \n        }\n    }\n\n}\n","<div class=\"page-header justify-content-between\">\n    <div class=\"page-title\">{{\"PaymentHistory\" | translate}}</div>\n</div>\n<div class=\"list-wrapper\">\n    <div class=\"list-overflow\" (scroll)=\"onScroll($event)\">\n        <div class=\"list-body\">\n            <div class=\"suggest-list\">\n                <!-- <nz-spin *ngIf=\"loading\"></nz-spin> -->\n                <ng-container *ngIf=\"historynotfound == true\">\n                    <p class=\"text-center pt-4\" *ngIf=\"paymentHistory?.length == 0\">{{\"NoHistory\"