eOkSubscribe(): void {\n        this.isVisibleSubscribe = false;\n    }\n\n    handleCancelSubscribe(): void {\n        this.selectedIndex = null;\n        this.isVisibleSubscribe = false;\n    }\n    subscribeValue = 'A';\n    subscribeSelectPayment = 'wallet';\n\n    // Modal -- Subscribe -- selected view\n    subscribe = 'subscribeSelect';\n\n\n    GetMyProfile() {\n        this.profileService.GetProfileSub().subscribe((data) => {\n            if (data && data['success']) {\n                this.profile = data;\n                this.isblocked = this.profile.blocked;\n            }\n        });\n    }\n    showModalSubscribe(usr, i): void {\n        this.subscribe = 'subscribeSelect'\n        this.renew_id = i;\n        this.selcteduser = usr;\n        this.selectedUserOnTimeCoupon = usr;\n        this.isVisibleSubscribe = true;\n    }\n\n    showSubscribe(usr,i):void { \n        this.subscribeProfile = usr;\n    }\n\n    getsubscribeValue() {\n        if (this.subscribeValue == 'A') {\n            this.subscription_price = this.selcteduser.subscription_price;\n        } else if (this.subscribeValue == 'B') {\n            this.subscription_price = this.selcteduser.subscription_price_6m;\n        } else if (this.subscribeValue == 'C') {\n            this.subscription_price = this.selcteduser.subscription_price_1yr;\n        }\n        else if (this.subscribeValue == 'D') {\n            this.subscription_price = this.selcteduser.subscription_price_3m;\n        } else {\n            this.subscription_price = this.selcteduser.subscription_price;\n        }\n    }\n    getCouponList() {\n        this.offersservice.GetAvaiCoupons().subscribe((data) => {\n            if (data['success']) {\n                this.listOfData = data['data'];\n                if (this.subscribeValue == 'A') {\n                    this.payPrice = this.selcteduser.subscription_price;\n                } else if (this.subscribeValue == 'B') {\n                    this.payPrice = this.selcteduser.subscription_price_6m;\n                } else if (this.subscribeValue == 'C') {\n                    this.payPrice = this.selcteduser.subscription_price_1yr;\n                }\n                else if (this.subscribeValue == 'D') {\n                    this.payPrice = this.selcteduser.subscription_price_3m;\n                }\n                let x = this.listOfData.find(\n                    (ob) => ob.couponcode == this.ccode\n                );\n                if (x) {\n                    this.coupon_id = x._id;\n                    this.coupon_per = x.percentage;\n                    this.errmessage = '';\n                    this.dis_amount = (this.payPrice * x.percentage) / 100;\n                } else {\n                    this.dis_amount = 0;\n                    this.errmessage = 'NO such Coupon Found';\n                }\n            }\n        });\n    }\n\n\n    ExtendUSer(i) {\n        // console.log(this.subscription_price);\n        let paymentprice = 0;\n        let paymentmonth = 0;\n        this.payPrice = this.subscription_price;\n        if (this.subscribeValue == 'A') {\n            paymentmonth = 1;\n        } else if (this.subscribeValue == 'B') {\n            paymentmonth = 6;\n        } else if (this.subscribeValue == 'C') {\n            paymentmonth = 12;\n        }\n        else if (this.subscribeValue == 'D') {\n            paymentmonth = 3;\n        }\n        let val = {\n            follower_id: this.selcteduser._id,\n            months: paymentmonth,\n            amount: this.payPrice,\n            coupon_code: this.ccode,\n            coupon_id: this.coupon_id,\n        };\n        this.profileService.RenewSubscribeUser(val).subscribe((res) => {\n            if (res['success']) {\n                this.message.success(res['message']);\n                this.selectMenu = 'all';\n                this.handleCancelSubscribe();\n            } else {\n                this.subscription_price = 0;\n                this.message.error(res['message']);\n            }\n        });\n    }\n\n    GetList() {\n        this.favService.GetList().subscribe((data) => {\n            if (data['success']) {\n                this.list = data['data'];\n            }\n        });\n    }\n    SearchMenu() {\n        if (this.selectMenu == \"expire\") {\n            this.getfollowing = [];\n            this.listService.GetexpiryFollowing().subscribe(result => {\n                let val = result['data'];\n                this.loading = false;\n                val.forEach(element => {\n                    this.getfollowing.push(element);\n                });\n\n            });\n        }\n        else if (this.selectMenu == \"active\") {\n            // console.log('true')\n            this.getfollowing = [];\n            this.listService.GetFollowing().subscribe(result => {\n                let val = result['data'];\n                this.loading = false;\n                val.forEach(element => {\n                    this.getfollowing.push(element);\n                });\n\n            });\n        }\n        else if(this.selectMenu == 'follow'){\n            this.getfollowing = [];\n            this.listService.GetFollowList().subscribe(result => {\n                let val = result['data'];\n                this.loading = false;\n                val.forEach(element => {\n                    this.getfollowing.push(element);\n                });\n\n            });\n        }\n        else {\n\n            this.getfollowing = [];\n            this.listService.GetAllFollowing().subscribe(result => {\n                let val = result['result'];\n                this.loading = false;\n                val.forEach(element => {\n                    this.getfollowing.push(element);\n                });\n\n            });\n        }\n        if (this.selectfollowMenu == 'expired') {\n            this.getfollower = [];\n            this.listService.GetexpiryFollower().subscribe(result => {\n                let val = result['data'];