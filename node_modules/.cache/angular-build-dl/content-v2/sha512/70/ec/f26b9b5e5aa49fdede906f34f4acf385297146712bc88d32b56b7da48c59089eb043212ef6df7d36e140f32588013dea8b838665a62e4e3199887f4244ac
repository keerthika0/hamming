           const duration = time - this.swipeLightboxImgTime;\n            if (duration < 1000 //\n                && Math.abs(direction[0]) > 30 // Long enough\n                && Math.abs(direction[0]) > Math.abs(direction[1] * 3)) { // Horizontal enough\n                if (direction[0] < 0) {\n                    this.nextImageLightbox();\n                }\n                else {\n                    this.prevImageLightbox();\n                }\n            }\n        }\n    }\n}\nSliderLightboxComponent.fac = function SliderLightboxComponent_Factory(t) { return new (t || SliderLightboxComponent)(ngcc0.directiveInject(ngcc0.ChangeDetectorRef), ngcc0.directiveInject(ngcc2.DomSanitizer), ngcc0.directiveInject(ngcc0.ElementRef), ngcc0.directiveInject(DOCUMENT)); };\nSliderLightboxComponent.cmp = ngcc0.defineComponent({ type: SliderLightboxComponent, selectors: [[\"slider-lightbox\"]], viewQuery: function SliderLightboxComponent_Query(rf, ctx) { if (rf & 1) {\n        ngcc0.viewQuery(_c9, true);\n        ngcc0.viewQuery(_c10, true);\n    } if (rf & 2) {\n        var _t;\n        ngcc0.queryRefresh(_t = ngcc0.loadQuery()) && (ctx.lightboxDiv = _t.first);\n        ngcc0.queryRefresh(_t = ngcc0.loadQuery()) && (ctx.lightboxImageDiv = _t.first);\n    } }, hostBindings: function SliderLightboxComponent_HostBindings(rf, ctx) { if (rf & 1) {\n        ngcc0.listener(\"resize\", function SliderLightboxComponent_resize_HostBindingHandler($event) { return ctx.onResize($event); }, false, ngcc0.resolveWindow)(\"keyup\", function SliderLightboxComponent_keyup_HostBindingHandler($event) { return ctx.handleKeyboardEvent($event); }, false, ngcc0.resolveDocument);\n    } }, inputs: { images: \"images\", videoAutoPlay: \"videoAutoPlay\", direction: \"direction\", paginationShow: \"paginationShow\", infinite: \"infinite\", arrowKeyMove: \"arrowKeyMove\", showVideoControls: \"showVideoControls\", imageIndex: \"imageIndex\", show: \"show\", animationSpeed: \"animationSpeed\" }, outputs: