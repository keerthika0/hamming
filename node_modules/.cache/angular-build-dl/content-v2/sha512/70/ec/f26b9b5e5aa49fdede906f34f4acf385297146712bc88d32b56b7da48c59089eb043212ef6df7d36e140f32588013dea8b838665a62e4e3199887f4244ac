erProfile = data['data'];\n                    let user = [];\n                    let image = [];\n                    user = data['data']['posts'];\n                    let userid = this.userProfile['_id'];\n\n                    for (let i = 0; i < user.length; i++) {\n                        this.likeLength = user[i].likes.length;\n                    }\n\n                    if (this.userProfile.followers.some(function (el) { return el.user_id == localStorage.getItem('user_id') && el.blocked == true && el.blocked_by == userid })\n                        || this.userProfile.following.some(function (el) { return el.follower_id == localStorage.getItem('user_id') && el.blocked == true && el.blocked_by == userid })) {\n                        this.userIsBlocked = true;\n                    }\n                    else {\n                        this.userIsBlocked = false;\n                    }\n                    for (let i = 0; i < user.length; i++) {\n                        for (let j = 0; j < user[i].file.length; j++) {\n                            if (\n                                user[i].file[j]['type'] == 'image/png' ||\n                                user[i].file[j]['type'] == 'image/jpg' ||\n                                user[i].file[j]['type'] == 'image/svg'\n                            ) {\n                                image.push(user[i].file[j]);\n                            }\n                        }\n                    }\n                    this.imageCount = image.length;\n                    this.GetFavList(this.userProfile._id);\n                    this.isBlocked = data['isBlocked'];\n                    this.subscribeProfile = data['data'];\n                    if (data['follower'][0] == 1) this.isFollowing = true;\n                    this.loadingStatus = 1;\n                    this.callTimings = data['data'].calltimings;\n                } else {\n                    this.loadingStatus = 0;\n                }\n                if (