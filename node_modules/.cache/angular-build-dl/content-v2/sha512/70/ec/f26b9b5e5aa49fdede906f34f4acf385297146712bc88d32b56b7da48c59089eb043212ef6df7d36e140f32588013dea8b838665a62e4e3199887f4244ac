guages = [];\n\n    g2f = { qr: '', code: '' };\n\n    blockList = [];\n    blockListTemp = [];\n\n    inputValue = '';\n    searchTextChanged = new Subject<string>();\n    searching = false;\n\n    countryCode = [];\n    country_code;\n    flagurl = \"https://www.countryflags.io/\";\n    endurl = \"/flat/64.png\";\n    country_name;\n    imageFlag;\n    twitter;\n    instagram;\n    mobile;\n    email;\n\n    constructor(\n        private fb: FormBuilder,\n        private profileService: ProfileService,\n        private authService: AuthService,\n        private notify: NotifyService,\n        private translate: TranslateService,\n    ) { }\n    isblocked: any;\n    ngOnDestroy(): void {\n        this.subscriptions.forEach((item) => item.unsubscribe());\n    }\n\n    ngOnInit(): void {\n        let printSrc = new PrintScreens;\n        this.countryCode = CountryDropdown;\n        printSrc.PrintScr();\n        this.profileService.TriggerProfile();\n        this.GetMyProfile();\n\n        this.passwordForm = this.fb.group({\n            password: [null, [Validators.required]],\n            newPassword: [\n                null,\n                [\n                    Validators.required,\n                    Validators.pattern(\n                        /^(?=.*[a-z])(?=.*[A-Z])(?=.*[0-9])(?=.*[!@#\\$%\\^&\\*\\/])(?=.{8,})/\n                    ),\n                    this.passwordValidator,\n                ],\n            ],\n            confirmPassword: [\n                null,\n                [Validators.required, this.confirmValidator],\n            ],\n        });\n\n        this.subscriptions.push(\n            this.profileService.GetProfileSub().subscribe((data) => {\n                if (data && data['success']) {\n                    this.profile = data;\n                    this.twitter = data['personal_info']['twitter'];\n                    this.instagram = data['personal_info']['instagram'];\n                    this.mobile = data['mobile'];\n                    this.email = data['email'];\n                    this.country_name = data['country_name']\n                    // this.SetProfileForm(data);\n                    this.SetProfileForm(data);\n                    // this.SetAccountForm(data);\n                    this.profileLoading = false;\n                }\n            })\n        );\n\n        this.BlockList();\n\n        this.subscriptions.push(\n            this.searchTextChanged.pipe(debounceTime(300)).subscribe((val) => {\n                this.searching = true;\n                if (val) {\n                    this.blockList = this.blockListTemp.filter(\n                        (item) =>\n                            item.follower_id.user_name\n                                .toLowerCase()\n                                .includes(val.toLowerCase()) ||\n                            item.follower_id.display_name\n                                .toLowerCase()\n                                .includes(val.toLowerCase())\n                    );\n                } else {\n                    this.blockList = this.blockListTemp;\n                }\n                this.searching = false;\n            })\n        );\n        this.getprofile();\n      \n        this.accountForm = this.fb.group({\n            email: [this.email, [Validators.required, Validators.email]],\n            Country_name: [this.country_name],\n            // country_code:[data.country_code],\n            mobile: [\n                this.mobile,\n                [\n                    Validators.required,\n                    Validators.pattern('([0-9]{7,15})'),\n                ],\n            ],\n            instagram: [\n                this.instagram,\n                [Validators.minLength(2)],\n            ],\n            twitter: [\n                this.twitter,\n                [ Validators.minLength(2)],\n            ],\n        });\n        this.countryChange();\n        this.GetLanguages();\n    }\n    GetMyProfile() {\n        this.profileService.GetProfileSub().subscribe((data) => {\n            if (data && data['success']) {\n                this.isblocked = data['blocked'];\n\n            }\n        });\n    }\n\n    getprofile(){\n        this.profileService.GetProfileSub().subscribe((data) => {\n            if (data && data['success']) {\n                 this.profile = data;\n                this.twitter = data['personal_info']['twitter'];\n                this.instagram = data['personal_info']['instagram'];\n                this.mobile = data['mobile'];\n                this.email = data['email'];\n                this.country_name = data['country_name'];\n                // this.SetProfileForm(data);\n                this.SetProfileForm(data);\n                // this.SetAccountForm(data);\n                this.profileLoading = false;\n            }\n        })\n    }\n\n    BlockList() {\n        this.profileService.BlockList().subscribe((data) => {\n            if (data['success']) {\n                this.blockList = data['data'];\n                this.blockListTemp = data['data'];\n            }\n        });\n    }\n\n    Search(val) {\n        this.searchTextChanged.next(val);\n    }\n\n    GetLanguages() {\n        this.profileService.ListLanguage().subscribe((data) => {\n            if (data['success']) {\n                this.languages = data['data'];\n            }\n        });\n    }\n\n    UpdateLanguage(language) {\n        let temp = this.profile.language;\n        this.profile.language = '';\n        this.profileService\n            .ChangeLanguage({ language })\n            .pipe(finalize(() => (this.profile.language = temp)))\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success('Language updated');\n                        temp = language;\n                        this.translate.use(validLanguage(temp));\n                        localStorage.setItem('locale', temp);\n                        document.documentElement.lang = temp;\n\n                    } else {\n                        this.notify.error(data['Language not updated']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    confirmValidator = (control: FormControl): { [s: string]: boolean } => {\n        if (!control.value) {\n            return { error: true, required: true };\n        } else if (\n            control.value !== this.passwordForm.controls.newPassword.value\n        ) {\n            return { confirm: true, error: true };\n        }\n        return {};\n    };\n\n    passwordValidator = (control: FormControl): { [s: string]: boolean } => {\n        if (!control.value) {\n            return { error: true, required: true };\n        }\n        this.passwordForm.controls.confirmPassword.updateValueAndValidity();\n        return {};\n    };\n\n    SetProfileForm(data) {\n        this.profileForm = this.fb.group({\n            user_name: [\n                data.user_name,\n                [Validators.required, Validators.minLength(3)],\n            ],\n            display_name: [\n                data.display_name,\n                [Validators.required, Validators.minLength(3)],\n            ],\n            about: [data.about, [Validators.required, Validators.minLength(5)]],\n            location: [\n                data.location,\n                [Validators.required, Validators.minLength(2)],\n            ],\n        });\n    }\n\n    // SetAccountForm(data) {\n    //      this.accountForm = this.fb.group({\n    //         email: [data.email, [Validators.required, Validators.email]],\n    //         Country_name:[data.country_name],\n    //         // country_code:[data.country_code],\n    //         mobile: [\n    //             data.mobile,\n    //             [\n    //                 Validators.required,\n    //                 Validators.pattern('([0-9]{7,15})'),\n    //             ],\n    //         ],\n    //         instagram: [\n    //             data?.personal_info?.instagram,\n    //             [Validators.required, Validators.minLength(2)],\n    //         ],\n    //         twitter: [\n    //             data?.personal_info?.twitter,\n    //             [Validators.required, Validators.minLength(2)],\n    //         ],\n    //     });\n    // this.countryChange();\n    // }\n\n    UpdateProfile() {\n        if (this.profileForm.valid) {\n            this.profileService.UpdateProfile(this.profileForm.value).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.profileService.SetProfile(data['data']);\n                        // this.GetMyProfile();\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n        }\n    }\n\n    UpdateAccount() {\n        for (const i in this.accountForm.controls) {\n            this.accountForm.controls[i].markAsDirty();\n            this.accountForm.controls[i].updateValueAndValidity();\n        }\n\n        if (this.accountForm.valid) {\n            let val = {\n                email: this.accountForm.value.email,\n                Country_name: this.accountForm.value.Country_name,\n                country_code: this.country_code,\n                mobile: this.accountForm.value.mobile,\n                instagram: this.accountForm.value.instagram,\n                twitter: this.accountForm.value.twitter\n            }\n            this.profileService.UpdateAccount(val).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                       location.reload();\n                        \n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n        }\n    }\n\n    isLoading = false;\n\n    UpdatePassword() {\n        if (!this.passwordForm.valid) {\n            this.notify.error('Enter all values')\n        }\n        if (this.passwordForm.valid) {\n            this.isLoading = true;\n                   \n            const { password, newPassword } = this.passwordForm.value;\n            this.profileService\n                .ChangePassword({\n                    old_password: password,\n                    password: newPassword,\n                })\n                .subscribe(\n                    (data) => {\n                        if (data['success']) {\n                            this.notify.success(data['message']);\n                            this.passwordForm.reset();\n                            this.isLoading = false;\n                   \n                        } else {\n                            this.notify.error(data['message']);\n                            this.isLoading = false;\n                   \n                        }\n                    },\n                    (err) => {\n                        this.isLoading = false;\n                        this.notify.error(err.error['message']);\n                    }\n                );\n        }\n    }\n\n    ChangeNotificationStatus() {\n        this.profileService.ChangeNotificationStatus().subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.notify.success(data['message']);\n                    this.profile.notification_enable = !this.profile\n                        .notification_enable;\n                } else {\n                    this.notify.error(data['message']);\n                }\n            },\n            (err) => {\n                this.notify.error(err.error['message']);\n            }\n        );\n    }\n\n     enable = 0;\n\n    GetG2f() {\n        this.enable = 0;\n        this.authService.GetG2f().subscribe((data) => {\n            if (data['success']) {\n                this.g2f.code = data['data']['secret'];\n                this.g2f.qr = data['data']['img'];\n                this.isVisibleEnable2fa = true;\n            }\n        });\n    }\n\n    Notify() {\n        if(this.enable == 0){\n            this.notify.toast('copied', 'success');\n            this.enable = 1;\n        }\n        \n    }\n\n    EnableG2f(otp) {\n        this.authService.EnableG2f({ otp }).subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.notify.success(data['message']);\n                    this.profile.tfa_active = true;\n                    this.isVisibleEnable2fa = false;\n                    this.profileService.SetProfile({ tfa_active: true });\n                } else {\n                    this.notify.error(data['message']);\n                }\n            },\n            (err) => {\n                this.notify.error(err.error['message']);\n            }\n        );\n    }\n\n    DisableG2f(otp) {\n        this.authService.DisableG2f({ otp }).subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.notify.success(data['message']);\n                    this.profile.tfa_active = false;\n                    this.isVisibleDisable2fa = false;\n                    this.profileService.SetProfile({ tfa_active: false });\n                } else {\n                    this.notify.error(data['message']);\n                }\n            },\n            (err) => {\n                this.notify.error(err.error['message']);\n            }\n        );\n    }\n\n    UnBlock() {\n        this.profileService\n            .UnblockUser({ block_id: this.unBlock.id })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.blockList.splice(this.unBlock.indx, 1);\n                        this.blockListTemp = this.blockListTemp.filter(\n                            (item) => item._id != this.unBlock.id\n                        );\n                        this.isVisibleUnblock = false;\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    countryChange() {\n        if (this.accountForm.value.Country_name == null) {\n            for (let i = 0; i < this.countryCode[0].length; i++) {\n\n                if (\n                    this.countryCode[0][i].code ==\n                    this.country_name\n                ) {\n                    this.imageFlag =\n                        this.flagurl + this.countryCode[0][i].code + this.endurl;\n                    this.country_name = this.countryCode[0][i].code;\n                    this.country_code = this.countryCode[0][i].dial_code\n                }\n            }\n        }\n        else {\n             for (let i = 0; i < this.countryCode[0].length; i++) {\n\n                if (\n                    this.countryCode[0][i].code ==\n                    this.accountForm.value.Country_name\n                ) {\n                    this.imageFlag =\n                        this.flagurl + this.countryCode[0][i].code + this.endurl;\n                    this.country_name = this.countryCode[0][i].code;\n                    this.country_code = this.countryCode[0][i].dial_code\n                }\n            }\n        }\n    }\n}\n","<div class=\"main-layout\">\n    <div *ngIf=\"isblocked\">\n        <div class=\"layout-inner-body d-flex justify-content-center\">\n            <nz-result nzStatus=\"info\" nzTitle=\"User blocked\">\n                <div nz-result-extra>\n                    {{\"Blocked\" | translate}}\n                </div>\n            </nz-result>\n        </div>\n    </div>\n    <div class=\"layout-body\" *ngIf=\"!isblocked\">\n        <div *ngIf=\"profileLoading\" class=\"d-flex justify-content-center align-items-center\" style=\"height: 50vh;\">\n            <nz-spin></nz-spin>\n        </div>\n        <div class=\"layout-inner-body\" *ngIf=\"!profileLoading\">\n            <!-- Edit Layout -- Start -->\n            <div class=\"edit-layout\">\n                <div class=\"page-header\">\n                    <div class=\"page-title\">{{\"Setting\" | translate}}</div>\n                </div>\n                <div class=\"p-15 h5 mb-0 border-bottom setting-user\">\n                    @{{ profile?.user_name }}\n                </div>\n                <div class=\"edit-list\">\n\n                    <a class=\"edit-item\" (click)=\"tab = 1\">\n                        <div class=\"title\">\n                            {{\"Account\" | translate}}\n                        </div>\n                        <div>\n                            <i-feather name=\"chevron-right\"></i-feather>\n                        </div>\n                        <div class=\"active-item\" *ngIf=\"tab == 1\"></div>\n                    </a>\n                    <a class=\"edit-item\" (click)=\"tab = 2\">\n                        <div class=\"title\">\n                            {{\"Security\" | translate}}\n                        </div>\n                        <div>\n                            <i-feather name=\"chevron-right\"></i-feather>\n                        </div>\n                        <div class=\"active-item\" *ngIf=\"tab == 2\"></div>\n                    </a>\n                    <a class=\"edit-item\" (click)=\"tab = 3\">\n                        <div class=\"title\">\n                           