\" *ngIf=\"(files.type).split('/')[0] == 'video'  \">\n                                    <div class=\"post-video-body\" (dblclick)=\"dblikethispost(post._id,f)\">\n                                        <video width=\"600\" controls>\n                                            <source src=\"{{files.url}}\">\n                                        </video>\n                                    </div>\n                                </div>\n                                <!-- Video Post --end -->\n                                <!-- Audio Post --start -->\n                                <div class=\"post-audio-wrapper\" *ngIf=\"(files.type).split('/')[0] == 'audio' \">\n                                    <div class=\"post-audio-body\" (dblclick)=\"dblikethispost(post._id,f)\">\n                                        <audio controls>\n                                            <source src=\"{{files.url}}\">\n                                        </audio>\n                                    </div>\n                                </div>\n                                <!-- Audio Post --end -->\n                            </ng-container>\n                        </ng-container>\n                        <!-- Single Post --end -->\n                        <!-- Multiple Post --start -->\n                        <ng-container *ngIf=\"post.file?.length > 1\">\n                            <div class=\"post-owl\">\n                                <owl-carousel-o [options]=\"customOptions\" #multiplepost>\n                                    <ng-template carouselSlide *ngFor=\"let files of post.file; let i = index\">\n                                        <div class=\"post-common-wrapper\">\n                                            <div class=\"post-common-body\" (dblclick)=\"dblikethispost(post._id,f)\">\n                                                <div class=\"post-common-type\">\n                                                    <div class=\"post-au