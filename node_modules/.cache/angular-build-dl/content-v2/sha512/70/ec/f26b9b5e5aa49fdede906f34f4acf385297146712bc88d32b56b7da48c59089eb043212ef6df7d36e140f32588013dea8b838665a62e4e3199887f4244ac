     } else {\n                    this.notify.error(data['message']);\n                }\n            },\n            (err) => {\n                this.notify.error(err.error['message']);\n            }\n        );\n    }\n\n    GetMyPosts() {\n        if (\n            !this.fetching &&\n            (this.totalPosts == 0 || this.totalPosts > this.posts.length)\n        ) {\n            this.fetching = true;\n            this.postService\n                .GetMyPosts(\n                    this.posts.length > 0\n                        ? {\n                            limit: this.limit,\n                            id: this.posts.slice(-1)[0]._id || null,\n                        }\n                        : { limit: this.limit }\n                )\n                .pipe(finalize(() => (this.fetching = false)))\n                .subscribe((data) => {\n                    if (data['success']) {\n                        if (data['data'].length > 0) {\n                            this.posts.push(...data['data']);\n                            this.totalPosts = data['count'];\n                        }\n                    }\n                });\n        }\n    }\n\n    SetSubscriptionForm(data: IProfile) {\n        this.subscriptionForm = this.fb.group({\n            subscription_price: [\n                data.subscription_price,\n                [Validators.required, Validators.min(0)],\n            ],\n            subscription_price_6m: [\n                data.subscription_price_6m,\n                [Validators.required, Validators.min(0)],\n            ],\n            subscription_price_3m: [\n                data.subscription_price_3m,\n                [Validators.required, Validators.min(0)],\n            ],\n            subscription_price_1yr: [\n                data.subscription_price_1yr,\n                [Validators.required, Validators.min(0)],\n            ],\n        });\n        this.oneMonth = data.subscription_price_status;\n        this.threeMonth = data.subscription_price_3m_status;\n        this.sixMonth = data.subscription_price_6m_status;\n        this.oneYear = data.subscription_price_1yr_status;\n    }\n\n\n    Updateprofile() {\n        for (const i in this.profileForm.controls) {\n            this.profileForm.controls[i].markAsDirty();\n            this.profileForm.controls[i].updateValueAndValidity();\n        }\n\n        this.profileService\n            .AuthDuplicate({\n                value: this.profileForm.value['user_name'],\n                type: 'user_name',\n            })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        if (data['found']) {\n                            this.user_name_error = true;\n                        } else {\n                            this.user_name_error = false;\n                            if (this.profileForm.valid) {\n                                const formData = new FormData();\n\n                                Object.keys(this.profileForm.value).forEach(\n                                    (item) => {\n                                        formData.append(\n                                            item,\n                                            this.profileForm.value[item]\n                                        );\n                                    }\n                                );\n\n                                Object.keys(this.files).forEach((item) => {\n                                    formData.append(item, this.files[item]);\n                                });\n                                formData.append('country_code', this.country_code);\n                                // formData.append('categoryname', this.category_name);\n                                // formData.append('sub_category', this.sub_cat);\n\n                                formData.append('subcategoryid', this.Sub_id);\n                                formData.append('category_id', this.