',\n        qrcode_url:'',\n        status:0\n    };\n    selectCoin='btc';\n\n    isVisibleDepositDetails=false;\n\n    showModalDepositDetails(type,i){\n\n        if(type == 'CRYPTO'){\n            this.isVisibleDepositDetails=true;\n            this.getPaymentDetails=this.depositHistory[i].coinpayment_id;\n            this.selectCoin=(this.getPaymentDetails.payment_type).toLowerCase();\n\n        }\n    }\n\n    constructor(private walletservice: WalletService,private notify: NotifyService) {}\n\n    ngOnInit(): void {\n        this.showdeposithistory();\n\n    }\n\n    closedetails(){\n        this.isVisibleDepositDetails=false;\n    }\n\n    showdeposithistory() {\n        this.walletservice.AllDepositHistory().subscribe( (data) => {\n            if (data['success']) {\n                this.loading=false;\n                this.depositHistory = data['data'];\n                let card_no = data['data']['card_number'];\n                if(this.depositHistory.length == 0){\n                    this.historynotfound = true;\n                }\n            }\n        },    \n        (err) => {\n            this.notify.error(err.error.message);\n        }\n        );\n    }\n\n    Notify() {\n        this.notify.success(\"Copied\", \"\");\n    }\n\n\n    handleCancelDepositCrypto(id):void{\n\n\n        this.walletservice.CancelPayment(id).subscribe( (data) => {\n            if (data['success']) {\n                this.isVisibleDepositDetails = false;\n                this.notify.success(data['message']);\n                this.showdeposithistory();\n            }\n        },    \n        (err) => {\n            this.notify.error(err.error.message);\n        }\n        );\n        \n\n    }\n\n}\n","<div class=\"page-header justify-content-between\">\n    <div class=\"page-title\">{{\"DepositHistory\" | translate}}</div>\n</div>\n<div class=\"list-wrapper\">\n    <div class=\"list-overflow\">\n        <div class=\"list-body\">\n            <div class=\"suggest-list\">\n 