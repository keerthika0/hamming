                       }\n                    },\n                    (err) => {\n                        this.notify.error(err.error['message']);\n                    }\n                );\n        }\n    }\n\n    DisablePricing() {\n        let val = {\n            subscription_price_1yr_status: this.oneYear,\n            subscription_price_3m_status: this.threeMonth,\n            subscription_price_6m_status: this.sixMonth,\n            subscription_price_status: this.oneMonth\n        }\n        this.profileService\n            .UpdateSubscription(val)\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.profileService.SetProfile(data['data']);\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    EditPricing(i) {\n        this.isVisibleTiming = true;\n        this.duration_Value = this.profile.calltimings[i].duration;\n        this.price_Value = this.profile.calltimings[i].price;\n        this.pricing_id = this.profile.calltimings[i]._id;\n    }\n\n    cancelPricing() {\n        this.isVisibleTiming = false;\n        this.duration_Value = 0;\n        this.price_Value = 0;\n        this.pricing_id = '';\n    }\n    visiblePricing() {\n        this.duration_Value = null;\n        this.price_Value = null;\n        this.pricing_id = '';\n        this.isVisibleTiming = true;\n    }\n\n    CallPricing(duration, price) {\n       \n       \n      if (this.pricing_id && this.pricing_id != '') {\n            if (duration <= 0 || price <= 0) {\n                this.notify.error('Enter correct value')\n            }\n            else {\n                this.profileService\n                    .UpdateCallPricing({\n                        duration: duration,\n                        price: price,\n                        time_id: this.pricing_id,\n                    })\n                    .subscribe(\n                        (data) => {\n                            if (data['success']) {\n                                this.notify.success(data['message']);\n                                this.profile.calltimings = data['data'].calltimings;\n                                this.cancelPricing();\n                            } else {\n                                this.notify.error(data['message']);\n                                this.cancelPricing();\n                            }\n                        },\n                        (err) => {\n                            this.notify.error(err.error.message);\n                            this.cancelPricing();\n                        }\n                    );\n            }\n        } else {\n\n            let callprice = this.profile.calltimings;\n            if (callprice.length == 0) {\n                this.profileService\n                    .AddCallPricing({ duration: duration, price: price })\n                    .subscribe(\n                        (data) => {\n                            if (data['success']) {\n                                this.notify.success(data['message']);\n                                this.profile.calltimings = data['data'].calltimings;\n                                this.cancelPricing();\n                            }\n                        },\n                        (err) => {\n                            this.notify.error(err.error.message);\n                            this.cancelPricing();\n                        }\n                    );\n            }\n\n            for (let i = 0; i < callprice.length; i++) {\n                if (callprice[i].duration == duration || callprice[i].price == price) {\n                    this.notify.error('Entered value is already present');\n                    return true;\n                }\n                else {\n                    this.profileService\n                        .AddCallPricing({ duration: duration, price: price })\n                        .subscribe(\n                            (data) => {\n                                if (data['success']) {\n                                    this.notify.success(data['message']);\n                                    this.profile.calltimings = data['data'].calltimings;\n                                    this.cancelPricing();\n                                }\n                            },\n                            (err) => {\n                                this.notify.error(err.error.message);\n                                this.cancelPricing();\n                            }\n                        );\n                    return true;\n                }\n            }\n\n\n        }\n    }\n\n    DeletePricing(id, i) {\n        if (id) {\n            this.profileService.DeleteCallPricing({ time_id: id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.profile.calltimings.splice(i, 1);\n                        this.notify.success('Deleted successfully');\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        }\n    }\n\n    PayPremium() {\n        this.subscriptionService\n            .PayPremium({\n                duration: this.selectedPackage?.validity,\n                price: this.selectedPackage?.price,\n                premium_package_id: this.selectedPackage?._id,\n            })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.profileService.TriggerProfile();\n                        this.isVisiblePremium = false;\n                        this.GetProfile();\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    SelectPackage(indx) {\n        this.selectedPackage = this.packages[indx];\n    }\n\n    Country() {\n        if (this.profileForm.value.Country_name == null) {\n            for (let i = 0; i < this.countryCode[0].length; i++) {\n\n                if (\n                    this.countryCode[0][i].code ==\n                    this.country_name\n                ) {\n                    this.imageFlag =\n                        this.flagurl + this.countryCode[0][i].code + this.endurl;\n                    this.country_name = this.countryCode[0][i].code;\n                    this.country_code = this.countryCode[0][i].dial_code\n                }\n            }\n        }\n        else {\n            for (let i = 0; i < this.countryCode[0].length; i++) {\n\n                if (\n                    this.countryCode[0][i].code ==\n                    this.profileForm.value.Country_name\n                ) {\n                    this.imageFlag =\n                        this.flagurl + this.countryCode[0][i].code + this.endurl;\n                    this.country_name = this.countryCode[0][i].code;\n                    this.country_code = this.countryCode[0][i].dial_code\n                }\n            }\n        }\n\n    }\n}\n","<div class=\"main-layout\">\n    <div *ngIf=\"isblocked\">\n        <div class=\"layout-inner-body d-flex justify-content-center\">\n            <nz-result nzStatus=\"info\" nzTitle=\"User blocked\">\n                <div nz-result-extra>\n                    {{\"Blocked\" | translate}}\n                </div>\n            </nz-result>\n        </div>\n    </div>\n    <div class=\"layout-body\" *ngIf=\"!isblocked\">\n        <nz-spin *ngI