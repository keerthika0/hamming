amounts mb-4\">\n                    <div class=\"walletcredits-text\">\n                        <h5 class=\"mb-0\">{{\"Balance\" |translate}}</h5>\n                    </div>\n                    <div class=\"walletcredits-balance\">\n                        <h4 class=\"mb-0 text-primary\">{{walletBalance | number: \"1.2-2\"}}</h4>\n                    </div>\n                </div>\n                <div class=\"walletcredits-addfund pb-4\">\n                    <nz-form-label>{{\"AddFund\" | translate}}</nz-form-label>\n                    <input nz-input placeholder=\"\" type=\"number\" value=\"{{amount}}\" name=\"amount\" id=\"amount\"\n                        #depamount [formControl]=\"deposit_amount\" />\n                    <div *ngIf=\"deposit_amount.hasError('pattern')\" style=\"color:red\">\n                        {{\"InvalidAmt\" | translate}}.\n                    </div>\n\n                    <p class=\"text-muted text-md pt-1\">*{{\"MinDeposit\" | translate}}</p>\n                    <div class=\"text-right mt-4\">\n                        <button nz-button [nzType]=\"'primary'\" [disabled]=\"deposit_amount.invalid\" nzSize=\"large\"\n                            (click)=\"showModalDeposit(depamount.value)\">{{\"Deposit\" | translate}}</button>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</div>\n<!-- Modal -- Deposit -->\n<nz-modal nzWrapClassName=\"vertical-center-modal premium-modal\" [(nzVisible)]=\"isVisibleDeposit\" [nzFooter]=\"null\"\n    nzClosable=\"false\">\n    <div *ngIf=\"deposit === 'paytype'\">\n        <div class=\"premium-modal-header\">\n            <img src=\"../../../../../assets/img/app/icon/pay-wallet.svg\" alt=\"wallet\" draggable=\"false\">\n        </div>\n        <div class=\"premium-modal-body\">\n            <h5 class=\"text-primary text-center mb-3\">{{\"AddtoWallet\" | translate}} {{addAmount}}</h5>\n            <div class=\"or-login my-3\">\n                <span cl