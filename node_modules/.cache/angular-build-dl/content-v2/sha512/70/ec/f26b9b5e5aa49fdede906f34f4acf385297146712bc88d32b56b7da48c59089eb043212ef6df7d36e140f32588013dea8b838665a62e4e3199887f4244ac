leBlockCard = false;\n    showModalBlockCard(): void {\n        this.isVisibleBlockCard = true;\n    }\n\n    handleOkBlockCard(): void {\n        this.isVisibleBlockCard = false;\n    }\n\n    handleCancelBlockCard(): void {\n        this.isVisibleBlockCard = false;\n    }\n\n    constructor(private fb: FormBuilder, private walletservice: WalletService, private notify: NotifyService, private router: Router\n    ) { }\n\n\n    ngOnInit(): void {\n        this.GetCardList();\n        this.AddCardForm = this.fb.group({\n            number: [null, [Validators.required, Validators.maxLength(16)]],\n            month: [null, [Validators.required, Validators.max(12), Validators.min(1)]],\n            cvc: [null, [Validators.required, Validators.minLength(3), Validators.maxLength(4)]],\n            year: [null, [Validators.required, Validators.min(this.minyear), Validators.max(this.maxyear)]],\n            street: [null, [Validators.required]],\n            email: [null, [Validators.required, Validators.email]],\n            city: [null, [Validators.required]],\n            state: [null, [Validators.required]],\n            pincode: [null, [Validators.required]],\n            country: [null, [Validators.required]],\n            agree: [false, [Validators.requiredTrue]],\n        });\n    }\n\n    GetCardList() {\n        this.walletservice.GetCardList().subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.loading = false;\n                    this.listOfCard = data['data'];\n                    this.curCardList = data['data'];\n                    this.SortList(this.selectedValueCards);\n                    if (this.curCardList.length == 0) {\n                        this.cardnotfound = true;\n                    }\n                }\n            },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n\n\n\n    SortList(e) {\n        this.selectedValueCards = e;\n        if (e) {\n            this.listOfCard.sort((a, b) => b.length - a.length);\n        } else {\n            this.listOfCard.sort((a, b) => a.length - b.length);\n        }\n        this.ChangePage(this.pageNumber);\n    }\n\n    ChangePage(e) {\n        this.pageNumber = e;\n        this.curCardList = this.listOfCard.slice(\n            (e - 1) * this.pageSize,\n            (e - 1) * this.pageSize + this.pageSize\n        );\n    }\n\n    AddNewCard() {\n\n        for (const i in this.AddCardForm.controls) {\n            this.AddCardForm.controls[i].markAsDirty();\n            this.AddCardForm.controls[i].updateValueAndValidity();\n        }\n        // console.log(this.AddCardForm);\n        if (this.AddCardForm.valid) {\n            const {\n                number,\n                month,\n                cvc,\n                year,\n                email,\n                street,\n                city,\n                state,\n                pincode,\n                country,\n            } = this.AddCardForm.value;\n            this.walletservice\n                .AddNewCard({\n                    number: number,\n                    month: month,\n                    cvc: cvc,\n                    year: year,\n                    email: email,\n                    street: street,\n                    city: city,\n                    state: state,\n                    pincode: pincode,\n                    country: country,\n                })\n                .subscribe(\n                    (data) => {\n                        if (data['success']) {\n                            this.notify.success(data['message']);\n                            this.AddCardForm.reset();\n                            this.GetCardList();\n                            this.tab = 0;\n\n                        } else {\n                            this.notify.error(data['message']);\n\n                        }\n                    },\n                    (err) => {\n                        this.notify.error(err.error['message']);\n\n                    }\n                );\n        }\n    }\n\n\n    showCardDetails(i) {\n        // console.log(i);\n        this.tab = 2;\n        this.viewcard.number = this.curCardList[i].number;\n        this.viewcard.month = this.curCardList[i].month;\n        this.viewcard.year = this.curCardList[i].year;\n        this.viewcard.brand = this.curCardList[i].brand;\n        this.viewcard.id = this.curCardList[i]._id;\n        this.DepositHistory(this.curCardList[i].card_id);\n    }\n\n    DepositHistory(id) {\n        this.walletservice.DepositHistoryByCard(id).subscribe((data) => {\n            if (data['success']) {\n                this.viewCardHistory = data['data'];\n            }\n        },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    removeCard(id) {\n        this.walletservice.RemoveCard(id).subscribe((data) => {\n            if (data['success']) {\n                this.notify.success(data['message']);\n                this.router.navigate(['/wallet']);\n                this.tab = 0;\n                this.GetCardList();\n            }\n        },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n\n}\n","<div *ngIf=\"tab==0\">\n    <div class=\"page-header justify-content-between\">\n        <div class=\"page-title\">{{\"Card\" | translate}}</div>\n        <a (click)=\"tab=1\" class=\"text-primary f-w-medium\">{{\"AddCard\" | translate}}</a>\n    </div>\n    <nz-spin *ngIf=\"loading\"></nz-spin>\n    <ng-container *ngIf=\"cardnotfound == true\">\n        <p class=\"text-center pt-4\" *ngIf=\"curCardList.length == 0\">{{\"NoCards\" | translate}}</p>\n    </ng-container>\n    <div class=\"edit-form\" (ngModelChange)=\"SortList($event)\">\n        <div nz-row [nzGutter]=\"16\">\n            <div nz-col nzXs=\"24\" nzSm=\"24\" nzMd=\"12\" nzLg=\"12\" nzXl=\"12\