For=\"about\">{{\"Insta\" | translate}}</nz-form-label>\n                            <nz-form-control [nzErrorTip]=\"instaErrorTpl\">\n                                <input nz-input name=\"about\" type=\"name\" [(ngModel)]=\"instagram\"\n                                    formControlName=\"instagram\" placeholder=\"john_doe\" />\n                                <ng-template #instaErrorTpl let-control>\n                                    <ng-container *ngIf=\"\n                                            control.hasError('minlength') ||\n                                            control.hasError('maxlength')\n                                        \">\n                                        {{\"InstaPattern\" | translate}}\n                                    </ng-container>\n                                    <ng-container *ngIf=\"control.hasError('required')\">\n                                        {{\"EnterInsta\" | translate}}\n                                    </ng-container>\n                                </ng-template>\n                            </nz-form-control>\n                        </nz-form-item>\n                        <nz-form-item>\n                            <nz-form-label nzFor=\"location\">{{\"Twitter\" | translate}}</nz-form-label>\n                            <nz-form-control [nzErrorTip]=\"twitterErrorTpl\">\n                                <input nz-input name=\"location\" type=\"name\" [(ngModel)]=\"twitter\"\n                                    formControlName=\"twitter\" placeholder=\"John Doe\" />\n                                <ng-template #twitterErrorTpl let-control>\n                                    <ng-container *ngIf=\"\n                                            control.hasError('minlength') ||\n                                            control.hasError('maxlength')\n                                        \">\n                                        {{\"TwitterPattern\" | translate}}\n                   