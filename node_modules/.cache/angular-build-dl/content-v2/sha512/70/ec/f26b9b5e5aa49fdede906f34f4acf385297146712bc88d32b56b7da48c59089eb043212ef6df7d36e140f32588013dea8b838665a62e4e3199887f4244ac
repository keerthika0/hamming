           this.selectedIndx = 0;\n                for (let i = 0; i < e.target.files.length; i++) {\n                    var reader = new FileReader();\n\n                    reader.onload = (data: any) => {\n                        this.preview.push({\n                            data: this.sanitizer.bypassSecurityTrustUrl(\n                                data.target.result\n                            ),\n                            type: e.target.files[i].type.split('/')[0],\n                        });\n                    };\n\n                    // let myBlob = new Blob(e.target.files[i]);\n                    \n                    // let mbo = new MediaBlobOperation(new MediaBlob(blob));\n                    // mbo.trim(240000, 360000);\n                    // mbo.finalize().then(function(mediaBlobs) {\n                    //     // mediaBlobs[0] will be the trimmed blob of 2 min duration\n                    // });\n                    reader.readAsDataURL(e.target.files[i]);\n                    this.files.push(e.target.files[i]);\n                }\n            }\n        }\n    }\n\n    imageCropped(event: ImageCroppedEvent) {\n        console.log(event);\n        this.croppedImage = event.base64;\n\n    }\n    imageLoaded() {\n        // show cropper\n    }\n    cropperReady() {\n        // cropper ready\n    }\n    loadImageFailed() {\n        // show message\n    }\n\n    cropDone() {\n        var reader = new FileReader();\n        this.preview.push({\n            data: this.sanitizer.bypassSecurityTrustUrl(\n                this.croppedImage\n            ),\n            type: \"image\",\n        });\n        var myBlob = new Blob([this.croppedImage], { type: \"image/png\" });\n\n        this.files.push(myBlob)\n\n        this.croppedImage = '';\n        this.imageChangedEvent = '';\n    }\n    cropCancel() {\n\n        this.croppedImage = '';\n        this.imageChangedEvent = '';\n    }\n\n    changeText = false;\n    take = 100;\n    completedtake = 100;\n    timerPaused = false;\n\n    stopscroll() {\n\n    }\n\n    replyStatus(message, obj) {\n\n        this.autoplay = true;\n        this.statusmessage = '';\n        this.statusmessage = null;\n\n        if (message != '' && message != null) {\n            this.homeservice\n                .ReplyStatus({\n                    status_id: this.userstatus[this.statusCurrentIndex]._id,\n                    reply: message,\n                    user_id: obj,\n                })\n                .subscribe(\n                    (data) => {\n                        if (data['success']) {\n                            this.notify.success(data['message']);\n                        }\n                    },\n                    (err) => {\n                        this.notify.error(err.error.message);\n                    }\n                );\n        }\n    }\n\n    Seen() {\n        this.homeservice\n            .Seen(this.userstatus[this.statusCurrentIndex]._id)\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n    }\n\n\n    StausSeenList() {\n        this.seenList = [];\n        this.homeservice\n            .StatusSeen(this.userstatus[this.statusCurrentIndex]._id)\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.seenList = data['data1'].reverse();\n                        this.SeenCount = this.seenList.length;\n                        if (this.seenList.length == 0) {\n                            this.zeroviews = true;\n                        }\n\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n    }\n\n    getPassedData(data: SlidesOutputData) {\n        this.activeSlides = data;\n    }\n\n    onScroll() {\n        if (this.count > 0) {\n            if (this.count >= this.limit) {\n                this.count = this.count - this.limit;\n            }\n            if (this.count < this.limit && this.count != 0) {\n                this.limit = this.count;\n                this.count = this.count - this.limit;\n            }\n            if (this.limit != 0) {\n                this.homeservice\n                    .GetFeedPostListById(this.id, this.limit)\n                    .subscribe(\n                        (data) => {\n                            if (data['success']) {\n                                for (var i = 0; i < data['data'].length; i++) {\n                                    if (data['data'][i].content) {\n                                        let content = data['data'][i].content;\n                                        data['data'][i].content = content.replace(/#(\\w+)/g, '<a  href=\"/hashtag/$1\">#$1</a>');\n                                    }\n                                    var datas = data['data'][i].likes;\n                                    data['data'][i].isLiked = false;\n                                    for (var j = 0; j < datas.length; j++) {\n                                        if (datas[j].user_id == this.user_id) {\n                                            data['data'][i].isLiked = true;\n                                        }\n                                    }\n\n                                    var datas = data['data'][i].bookmarks;\n                                    data['data'][i].isMarked = false;\n                                    for (var k = 0; k < datas.length; k++) {\n                                        if (datas[k].user_id == this.user_id) {\n                                            data['data'][i].isMarked = true;\n                                        }\n                                    }\n\n                                    this.listFeedPost.push(data['data'][i]);\n\n                                    this.id = data['data'].length - 1;\n                                    this.id = data['data'][this.id]._id;\n                                }\n                                // this.listFeedPost = data['data'];\n                                if (this.listFeedPost.length == 0) {\n                                    this.noofeedsfound = true;\n                                }\n                                this.profilecontent = this.listFeedPost[\n                                    'content'\n                                ];\n                            }\n                        },\n                        (err) => {\n                            this.notify.error(err.error.message);\n                        }\n                    );\n            }\n        }\n    }\n\n    hashTag(e) {\n        const element: HTMLElement = e.target;\n        if (element.nodeName === 'A') {\n            e.preventDefault();\n            const link = element.getAttribute('href');\n            this.router.navigate([link]);\n        }\n    }\n\n    GetFeedPostList() {\n        this.homeservice.GetFeedPostList().subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.count = data['count'] - 10;\n                    for (var i = 0; i < data['data'].length; i++) {\n                        if (data['data'][i].content) {\n                            let content = data['data'][i].content;\n                            data['data'][i].content = content.replace(/#(\\w+)/g, '<a  href=\"/hashtag/$1\">#$1</a>');\n                        }\n                        var datas = data['data'][i].likes;\n                        data['data'][i].isLiked = false;\n                        for (var j = 0; j < datas.length; j++) {\n                            if (datas[j].user_id == this.user_id) {\n                                data['data'][i].isLiked = true;\n                            }\n                        }\n\n                        var datas = data['data'][i].bookmarks;\n                        data['data'][i].isMarked = false;\n                        for (var k = 0; k < datas.length; k++) {\n                            if (datas[k].user_id == this.user_id) {\n                                data['data'][i].isMarked = true;\n                            }\n                        }\n\n                        this.listFeedPost.push(data['data'][i]);\n\n                        this.id = data['data'].length - 1;\n                        this.id = data['data'][this.id]._id;\n                    }\n\n                    // this.listFeedPost = data['data'];\n                    if (this.listFeedPost.length == 0) {\n                        this.noofeedsfound = true;\n                    }\n                    setTimeout(() => {\n                        this.postActive = false;\n                    }, 1200);\n                }\n            },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    ReportPost(id) {\n        this.homeservice.ReportPost({ post_id: id }).subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.notify.success(data['message']);\n                }\n            },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    likethispost(post_id, i) {\n        if (this.listFeedPost[i].isLiked == true) {\n            this.listFeedPost[i].isLiked = false;\n            this.homeservice.DisLikePost({ post_id: post_id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.listFeedPost[i].likes = data['data'].likes;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        } else {\n            this.listFeedPost[i].isLiked = true;\n            this.homeservice.LikePost({ post_id: post_id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.listFeedPost[i].likes = data['data'].likes;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        }\n    }\n\n    dblikethispost(post_id, i) {\n        if (this.listFeedPost[i].isLiked == false) {\n            this.listFeedPost[i].isLiked = true;\n            this.homeservice.LikePost({ post_id: post_id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.listFeedPost[i].likes = data['data'].likes;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        }\n    }\n\n    language: any;\n    catename: any;\n    GetMyProfile() {\n        this.profileService.GetProfile().subscribe((data) => {\n            if (data && data['success']) {\n                this.myProfie = data['data'];\n                this.display_name = this.myProfie.display_name;\n                this.isblocked = this.myProfie.blocked;\n                this.mybalance = this.myProfie.balance;\n                this.language = this.myProfie.language;\n                this.catename = this.myProfie.categoryname;\n            }\n        });\n    }\n\n    bookmarkthispost(post_id, i) {\n        if (this.listFeedPost[i].isMarked == true) {\n            this.listFeedPost[i].isMarked = false;\n            this.homeservice.RemoveBookMarkPost({ post_id: post_id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.listFeedPost[i].bookmarks = data['data'].bookmarks;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        } else {\n            this.listFeedPost[i].isMarked = true;\n            this.homeservice.BookMarkPost({ post_id: post_id }).subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.listFeedPost[i].bookmarks = data['data'].bookmarks;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        }\n    }\n\n    AddTips(id, amount, i) {\n        this.isVisibleTips = false;\n        this.addtipsTemp.post_id = id;\n        this.addtipsTemp.amount = amount;\n        this.addtipsTemp.i = i;\n        this.isVisibleTipsConfirmation = true;\n        this.tipsAmount = 0;\n    }\n\n    handleCancelTipsConfirm() {\n        this.isVisibleTipsConfirmation = false;\n        this.tipsAmount = 0;\n    }\n\n    ConfirmTipsPay() {\n        this.homeservice\n            .AddTips({\n                post_id: this.addtipsTemp.post_id,\n                amount: this.addtipsTemp.amount,\n            })\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.isVisibleTips = false;\n                        this.notify.success(data['message']);\n                        this.listFeedPost[this.addtipsTemp.i].tips_amount =\n                            data['data'].tips_amount;\n                        this.tips_post_id = '';\n                        this.tips_post_array_id = 0;\n                        this.isVisibleTipsConfirmation = false;\n                        this.GetMyProfile();\n                        this.tipsAmount = 0;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n    }\n\n    showLikes(id) {\n        this.homeservice.LikesList(id).subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.likeList = data['data'];\n                    this.isVisibleLikes = true;\n                    this.nolikesfound = true;\n                }\n            },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    showTipsrecieved(id) {\n        this.homeservice.TipsList(id).subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.tipsList = data['data'];\n                    this.isVisibleTipsrecieved = true;\n                    this.notipsfound = true;\n                }\n            },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    onFileChange(e) {\n        if (e.target.files && e.target.files[0]) {\n            this.selectedIndx = 0;\n            for (let i = 0; i < e.target.files.length; i++) {\n                var reader = new FileReader();\n\n                reader.onload = (data: any) => {\n                    this.preview.push({\n                        data: this.sanitizer.bypassSecurityTrustUrl(\n                            data.target.result\n                        ),\n                        type: e.target.files[i].type.split('/')[0],\n                    });\n                };\n\n                reader.readAsDataURL(e.target.files[i]);\n                this.files.push(e.target.files[i]);\n            }\n        }\n    }\n    errmsg = false;\n\n    onFileChangeStatus(e) {\n        if (e.target.files && e.target.files[0]) {\n            this.selectedStatusIndx = 0;\n            for (let i = 0; i < e.target.files.length; i++) {\n                var reader = new FileReader();\n                if (\n                    e.target.files[i].type == 'image/jpeg' ||\n                    e.target.files[i].type == 'image/png'\n                ) {\n                    this.errmsg = false;\n                } else {\n                    this.errmsg = true;\n                }\n                // if (e.target.files[i].type == \"image/jpeg\" || e.target.files[i].type == \"image/png\") {\n                reader.onload = (data: any) => {\n                    this.statuspreview.push({\n                        data: this.sanitizer.bypassSecurityTrustUrl(\n                            data.target.result\n                        ),\n                        type: e.target.files[i].type.split('/')[0],\n                    });\n                };\n\n                reader.readAsDataURL(e.target.files[i]);\n                this.files.push(e.target.files[i]);\n                // }\n\n                // else {\n                //     this.notify.error('upload only image')\n                // }\n            }\n        }\n    }\n\n    addEmoji(e) {\n        this.text += e.emoji.native;\n        // this.ontextchange(this.text);\n    }\n\n    addEmojis(e) {\n        this.statustext += e.emoji.native;\n        // this.onStatusText(this.statustext);\n    }\n\n    getHideValue(event) { }\n\n    CreatePost() {\n        // console.log('came inside');\n\n        if (!this.text && this.files.length == 0) {\n            // this.notify.error('Invalid');\n            return;\n        };\n        this.posting = true;\n        const formData = new FormData();\n\n        if (this.files.length > 0) {\n            for (let file of this.files) {\n                formData.append('file', file);\n            }\n        }\n        if (this.text.length > 140) {\n            this.notify.error('Post character is greater than 140');\n        } else {\n            formData.append('content', this.text);\n        }\n\n        formData.append('hide', this.hide);\n\n        this.homeservice\n            .CreatePost(formData)\n            .pipe(finalize(() => (this.posting = false)))\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.isVisibleNewPost = false;\n                        this.files = [];\n                        this.preview = [];\n                        this.text = '';\n                        this.hide = 'false';\n                    } else {\n                        this.hide = 'false';\n\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    CreateStatus() {\n        this.newstory = false;\n        if (this.files.length == 0) {\n            this.notify.error('Select File To Add Story');\n            return;\n        }\n\n        this.statusposting = true;\n        const formData = new FormData();\n\n        if (this.files.length > 0) {\n            for (let file of this.files) {\n                formData.append('file', file);\n            }\n        }\n\n        formData.append('content', this.statustext);\n\n        formData.forEach((item) => {\n            // console.log('file   \" ', item);\n        });\n\n        this.homeservice\n            .CreateStatus(formData)\n            .pipe(finalize(() => (this.statusposting = false)))\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.notify.success(data['message']);\n                        this.isVisibleNewPost = false;\n                        this.files = [];\n                        this.statuspreview = [];\n                        this.statustext = '';\n                        this.isVisibleNewStory = false;\n                        this.MyStatus();\n                    } else {\n                        this.notify.error(data['message']);\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error['message']);\n                }\n            );\n    }\n\n    RemoveFile() {\n        this.selectedIndx =\n            this.selectedIndx < this.preview.length - 1\n                ? this.selectedIndx++\n                : this.selectedIndx - 1;\n        this.files.splice(this.selectedIndx, 1);\n        this.preview.splice(this.selectedIndx, 1);\n    }\n\n    RemoveStatusFile() {\n        this.selectedStatusIndx =\n            this.selectedStatusIndx < this.statuspreview.length - 1\n                ? this.selectedStatusIndx++\n                : this.selectedStatusIndx - 1;\n        this.files.splice(this.selectedStatusIndx, 1);\n        this.statuspreview.splice(this.selectedStatusIndx, 1);\n        this.errmsg = false;\n    }\n\n    MyStatus() {\n        this.myStatus = [];\n        this.homeservice.MyStatus().subscribe(\n            (data) => {\n                if (data['success']) {\n                    for (var k = 0; k < data['data'].length; k++) {\n                        this.myStatus.push(data['data'][k].statusList);\n\n                        // this.statusvalue[k].secs = 0;\n\n                        this.imageObject.push({\n                            image: data['data'][k].statusList.file.file,\n                            title: 'Status',\n                            thumbImage: this.appUrl + this.profile_image,\n                            timer: 30,\n                            id: k,\n                        });\n                    }\n                }\n\n                // console.log(this.imageObject);\n            },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    StatusList() {\n        this.homeservice.StatusList().subscribe(\n            (data) => {\n                if (data['success']) {\n                    this.othersStatus = data['data'].sort(\n                        (a, b) =>\n                            new Date(b.updatedAt).getTime() -\n                            new Date(a.updatedAt).getTime()\n                    );\n                    setTimeout(() => {\n                        this.storyActive = false;\n                    }, 1200);\n                }\n            },\n            (err) => {\n                this.notify.error(err.error.message);\n            }\n        );\n    }\n\n    showStory() {\n        if (this.newstory != true && this.myStatus.length != 0) {\n            this.homeservice.getProfile().subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.userstatus_details = data['data'];\n                        this.whosStatus = 'YOURS';\n                        this.modal = true;\n                        this.userstatus = this.myStatus;\n                        this.viewEye = true;\n                        this.statusTotalIndex = this.myStatus.length - 1;\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n        } else {\n            this.isVisibleNewStory = true;\n            this.isVisibleStory = false;\n            this.modal = false;\n            this.newstory = true;\n        }\n    }\n\n    showOthersStory(i, dets, d) {\n        // this.getData(data);\n        this.whosStatus = 'OTHERS';\n        this.modal = true;\n        this.userstatus = i;\n        this.userstatus_details = dets;\n        this.userstatus_details_id = d;\n        this.viewEye = false;\n        this.statusTotalIndex = i.length - 1;\n\n    }\n\n    SeenCount;\n    isPlay: boolean;\n    currentIndex;\n\n    getData(data: SlidesOutputData) {\n        let j = this.userstatus_details_id;\n        this.activeSlides = data;\n        this.statusCurrentIndex = this.activeSlides.startPosition;\n        this.seconds = 0;\n        if (this.whosStatus !== 'YOURS') {\n            this.Seen();\n        }\n        this.timeago = this.userstatus[this.statusCurrentIndex].createdAt;\n        this.status_content = this.userstatus[this.statusCurrentIndex].content;\n        this.StausSeenList();\n        for (this.i = 0; this.i < this.userstatus.length; this.i++) {\n            if (this.i != this.statusCurrentIndex) {\n                this.userstatus[this.i].slide = false;\n            } else {\n                let videos = document.getElementsByTagName('video')\n                let audios = document.getElementsByTagName('audio')\n                this.userstatus[this.i].slide = true;\n                if(this.userstatus[this.i].file.type != 'video/mp4' || this.userstatus[this.i].file.type == 'video/mp4'){\n                    for(let i=0; i<videos.length;i++){\n                        videos[i].pause();\n                    }\n                }\n                if((this.userstatus[this.i].file.type).split('/')[0] == 'audio' || (this.userstatus[this.i].file.type).split('/')[0] != 'audio'){\n                   for(let i=0; i<audios.length;i++){\n                        audios[i].pause();\n                    }\n                }\n            }\n        }\n\n        if (this.modal == true) {\n            setTimeout(() => {\n                if (\n                    this.statusCurrentIndex == this.userstatus.length - 1 && this.timerPaused == false\n                ) {\n                    j++;\n                    if (this.othersStatus.length > j) {\n                        this.nextuser();\n                        this.i = 0;\n                    }\n                    else {\n                        // setTimeout(() => {\n                        this.modalClose();\n                        // }, 3000);   \n                    }\n                }\n\n            }, 30000);\n        }\n    }\n    // isPause = false;\n    currentTime: number;\n\n    setCurrentTime(data, i) {\n        console.log(data, i)\n        this.currentTime = data.target.currentTime;\n        var myVideo: any = document.getElementById(i);\n        if (this.currentTime > 5) {\n            myVideo.pause();\n        }\n        else {\n            myVideo.play();\n        }\n    }\n\n\n    onPause() {\n        this.timerPaused = true;\n    }\n\n    onResume() {\n        this.timerPaused = false;\n        setTimeout(() => {\n            if (\n                this.statusCurrentIndex == this.userstatus.length - 1 &&\n                this.timerPaused == false\n            ) {\n                this.nextuser();\n                this.i = 0;\n            }\n        }, 30000);\n    }\n\n    nextuser() {\n        console.log(this.othersStatus.length)\n        if (this.othersStatus.length == 0 || this.modal == false) {\n            this.modalClose()\n            return;\n        }\n        this.showOthersStory(\n            this.othersStatus[this.userstatus_details_id + 1].statusList,\n            this.othersStatus[this.userstatus_details_id + 1].user_id,\n            this.userstatus_details_id + 1,\n\n        );\n\n\n    }\n\n    prevuser() {\n        //     if (this.othersStatus.length == 0) {\n        //        this.modalClose()\n        //        return;\n        //    }\n        this.showOthersStory(\n            this.othersStatus[this.userstatus_details_id - 1].statusList,\n            this.othersStatus[this.userstatus_details_id - 1].user_id,\n            this.userstatus_details_id - 1,\n\n        );\n\n\n    }\n\n    deleteStatus() {\n        this.homeservice\n            .DeleteStatus(this.userstatus[this.statusCurrentIndex]._id)\n            .subscribe(\n                (data) => {\n                    if (data['success']) {\n                        this.handleCancelStoryReport();\n                        this.myStatus.splice(this.statusCurrentIndex, 1);\n\n                        if (this.myStatus.length == 0) {\n                            this.modal = false;\n                        }\n                    }\n                },\n                (err) => {\n                    this.notify.error(err.error.message);\n                }\n            );\n    }\n}\n","<div class=\"main