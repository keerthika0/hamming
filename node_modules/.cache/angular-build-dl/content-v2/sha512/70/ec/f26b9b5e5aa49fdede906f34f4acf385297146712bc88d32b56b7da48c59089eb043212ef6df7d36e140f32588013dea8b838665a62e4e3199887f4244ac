edule = false;\n    showModalCallSchedule(id): void {\n          if (this.User.followers.some(function (el) { return el.user_id == id && el.blocked == true })\n            || this.User.following.some(function (el) { return el.follower_id == id && el.blocked == true })) {\n            this.notify.error(this.translate.instant('Blocked'))\n        }\n        else if (this.User.following.some(function (el) { return el.follower_id === id && el.plan_status === 0 })) {\n          this.notify.error(this.translate.instant('Renew User'))\n        }\n        else {\n            this.callSchedule = 'callCreate';\n            this.isVisibleCallSchedule = true;\n            this.availableSlots = null;\n            this.callDuration = null;\n        }\n    }\n\n    handleOkCallSchedulet(): void {\n        this.isVisibleCallSchedule = false;\n    }\n\n    handleCancelCallSchedule(): void {\n        this.isVisibleCallSchedule = false;\n        this.callSchedule = 'callCreate';\n    }\n\n    submitForm(): void {\n        for (const i in this.validateForm.controls) {\n            this.validateForm.controls[i].markAsDirty();\n            this.validateForm.controls[i].updateValueAndValidity();\n        }\n    }\n\n    constructor(\n        private message: NzMessageService,\n        private sanitizer: DomSanitizer,\n        private translate: TranslateService,\n        private fb: FormBuilder,\n        private socketService: SocketService,\n        private messageService: MessageService,\n        private notify: NotifyService,\n        private cdref: ChangeDetectorRef,\n        private profileService: ProfileService,\n    ) { }\n\n    currentmessage;\n\n    ngOnInit(): void {\n        this.MessageTyping();\n        this.GetFollowers();\n        this.profileService.TriggerProfile();\n        this.GetMyProfile();\n        this.validateForm = this.fb.group({\n            userName: [null, [Validators.required]],\n            callDuration: [null, [Validators.required]],\n        });\n\n     