: Subscription[] = [];\n    profile: IProfile;\n\n    posts: IPost[] = [];\n    totalPosts = 0;\n    fetching = false;\n    limit = 5;\n    premium: any;\n    editLoading = false;\n\n    appUrl = Constants.baseUrl2;\n\n    isVisibleDeletePost = false;\n    isVisibleEditPost = false;\n\n    tempPost: IPost;\n\n    editFiles = [];\n    text = '';\n    hide = false;\n    Temphide = '';\n\n    followers = [];\n    following = [];\n\n    suggestionList: Array<Object>;\n    completeList: Array<Object>;\n    selcteduser: any = {};\n    selectedUserOnTimeCoupon: any = {};\n    isVisibleSubscribe = false;\n    showMore = true;\n    myProfie: IProfile;\n    subscribeProfile: IProfile;\n    isVisiblePremiumSub = false;\n    listOfData = [];\n    fcount: any;\n    ccode: any;\n    coupon_per: any = 0;\n    coupon_id: any;\n    payPrice: any = 0;\n    errmessage: any = ' ';\n    subscription_price = 0;\n    dis_amount: any = 0;\n    selectedIndex: any;\n    selectStatus = 'active';\n    renew_id: any;\n    followerCount = 0;\n    showModalSubscribe(usr, i): void {\n        this.subscribe = 'subscribeSelect';\n        this.renew_id = i;\n        this.selcteduser = usr;\n        this.selectedUserOnTimeCoupon = usr;\n        this.isVisibleSubscribe = true;\n    }\n\n    handleOkSubscribe(): void {\n        this.isVisibleSubscribe = false;\n    }\n\n    handleCancelSubscribe(): void {\n        this.selectedIndex = null;\n        this.isVisibleSubscribe = false;\n    }\n\n    // Radio -- Subscribe Pack\n    subscribeValue = 'A';\n    subscribeSelectPayment = 'wallet';\n    isblocked: any;\n    // Modal -- Subscribe -- selected view\n    subscribe = 'subscribeSelect';\n\n    // Modal -- Unsubscribe\n    isVisibleUnsubscribe = false;\n    showModalUnsubscribe(): void {\n        this.isVisibleUnsubscribe = true;\n    }\n\n    handleOkUnsubscribe(): void {\n        this.isVisibleUnsubscribe = false;\n    }\n\n    handleCancelUnsubscribe(): void {\n        this.isVisibleUnsubscribe = fals